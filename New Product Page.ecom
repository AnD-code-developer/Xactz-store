tton-next.ecom-swiper-button-disabled, .ecom-swiper-button-prev.ecom-swiper-button-disabled {\n    cursor: not-allowed;\n}\n.swiper-scrollbar {\n    border-radius: 2px;\n    height: 4px;\n    width: 450px;\n    background-color: #D9D9D9;\n    margin: 50px auto 0;\n    max-width: 90%;\n}\n.ecom-swiper-scrollbar-drag {\n    background: #E59060;\n}\n.ecom-collection__product-item {\n    overflow: visible;\n}\n@media (min-width:1025px){\n.ecom-collection__product-item--wrapper:hover .ecom-collection__product-form__actions {\n    opacity: 1;\n    visibility: visible;\n    transform: translateY(0);\n}\n.ecom-collection__product-form__actions {\n    transition: 0.4s;\n    transform: translateY(-10px);\n    opacity: 0;\n    visibility: hidden;\n}\n.ecom-swiper-navigation-position {\n    opacity: 0;\n    transition: 0.3s;\n    visibility: hidden;\n    z-index: 8;\n}\n.ecom-collection__product-container:hover .ecom-swiper-navigation-position {\n    opacity: 1;\n    visibility: visible;\n}\n}\n.ecom-product__wishlist.ecom-collection__action-hor-start.ecom-collection__action-ver-start a {\n    transform: translateX(-15px);\n    transition: 0.5s;\n    opacity: 0;\n    visibility: hidden;\n}\n.ecom-product__compare.ecom-collection__action-hor-start.ecom-collection__action-ver-start a {\n    transform: translateX(-30px);\n    transition: 0.5s;\n    opacity: 0;\n    visibility: hidden;\n}\n.ecom-collection__product-item:hover .ecom-product__wishlist.ecom-collection__action-hor-start.ecom-collection__action-ver-start a,\n.ecom-collection__product-item:hover .ecom-product__compare.ecom-collection__action-hor-start.ecom-collection__action-ver-start a{\n    transform: translateX(0);\n    opacity: 1;\n    visibility: visible;\n}\n.ecom-collection__product-price-wrapper {\n    gap: inherit;\n    column-gap: 5px;\n    flex-wrap: nowrap;\n}\n@media(min-width:768px) and (max-width:1024px){\n    .ecom-collection__product-item .ecom-product__wishlist.ecom-collection__action-hor-start.ecom-collection__action-ver-start a,\n.ecom-collection__product-item .ecom-product__compare.ecom-collection__action-hor-start.ecom-collection__action-ver-start a{\n    transform: translateX(0);\n    opacity: 1;\n    visibility: visible;\n}\n}\n@media(max-width:767px){\n    .ecom-collection__product-group-button-action {\n        display: none;\n    }\n}",
