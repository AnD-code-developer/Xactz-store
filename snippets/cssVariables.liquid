<style>
  {%- liquid     
    echo settings.header_type_primary | font_face: font_display: 'swap'    
    if settings.header_type_secondary != settings.header_type_primary
      echo settings.header_type_secondary | font_face: font_display: 'swap'
    endif     
    echo settings.base_type | font_face: font_display: 'swap'
    echo settings.label_type | font_face: font_display: 'swap'     
    assign body_font_bold = settings.base_type | font_modify: 'weight', 'bold'
    echo body_font_bold | font_face: font_display: 'swap'
    assign body_font_italic = settings.base_type | font_modify: 'style', 'italic'
    echo body_font_italic | font_face: font_display: 'swap'
    assign body_font_bold_italic = body_font_bold | font_modify: 'style', 'italic'
    echo body_font_bold_italic | font_face: font_display: 'swap'
    assign label_font_bold = settings.label_type | font_modify: 'weight', 'bold'
    echo label_font_bold | font_face: font_display: 'swap'
    assign label_font_italic = settings.label_type | font_modify: 'style', 'italic'
    echo label_font_italic | font_face: font_display: 'swap'
    assign label_font_bold_italic = label_font_bold | font_modify: 'style', 'italic'
    echo label_font_bold_italic | font_face: font_display: 'swap'
  %}
  :root {    
    --header:relative;    
    --headline_font_primary:{{ settings.header_type_primary.family }}, {{ settings.header_type_primary.fallback_families }};
    --headline_font_primary_weight:{{ settings.header_type_primary.weight }};
    --headline_font_primary_style:{{ settings.header_type_primary.style }};
    --headline_font_primary_uppercase:{% if settings.headline_uppercase_primary %}uppercase{% else %}none{% endif %};
    --headline_font_secondary:{{ settings.header_type_secondary.family }}, {{ settings.header_type_secondary.fallback_families }};
    --headline_font_secondary_weight:{{ settings.header_type_secondary.weight }};
    --headline_font_secondary_style:{{ settings.header_type_secondary.style }};
    --headline_font_secondary_uppercase:{% if settings.headline_uppercase_secondary %}uppercase{% else %}none{% endif %};
    --body_font:{{ settings.base_type.family }}, {{ settings.base_type.fallback_families }};
    --body_font_fallback:{{ settings.base_type.fallback_families }};        
    --body_font_weight:{{ settings.base_type.weight }};
    --body_font_weight_bold:{{ settings.base_type.weight | plus: 300 | at_most: 1000 }};
    --body_font_weight_lighter:lighter;
    --body_font_style:{{ settings.base_type.style }};
    --body_font_style_italic:italic;
    --label_font:{{ settings.label_type.family }}, {{ settings.label_type.fallback_families }};
    --label_font_fallback:{{ settings.label_type.fallback_families }};        
    --label_font_weight:{{ settings.label_type.weight }};
    --label_font_weight_bold:{{ settings.label_type.weight | plus: 300 | at_most: 1000 }};
    --label_font_weight_lighter:lighter;
    --label_font_style:{{ settings.label_type.style }};    
    --headline_font_size_12: calc(12px + (12 - 12) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_14: calc(12px + (14 - 12) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_16: calc(14px + (16 - 14) * ((100vw - 380px) / (1280 - 380)));    
    --headline_font_size_18: calc(16px + (18 - 16) * ((100vw - 380px) / (1280 - 380)));    
    --headline_font_size_24: calc(18px + (24 - 18) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_30: calc(24px + (30 - 24) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_36: calc(24px + (36 - 24) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_42: calc(36px + (42 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_48: calc(36px + (48 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_54: calc(36px + (54 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_60: calc(36px + (60 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_66: calc(36px + (66 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_72: calc(36px + (72 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_78: calc(36px + (78 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_84: calc(36px + (84 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_90: calc(36px + (90 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_96: calc(36px + (96 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_102: calc(36px + (102 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_108: calc(36px + (108 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_114: calc(36px + (114 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_120: calc(36px + (120 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_126: calc(36px + (126 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_132: calc(36px + (132 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_138: calc(36px + (138 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_144: calc(36px + (144 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_150: calc(36px + (150 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_156: calc(36px + (156 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_162: calc(36px + (162 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_168: calc(36px + (168 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_174: calc(36px + (174 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_180: calc(36px + (180 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_186: calc(36px + (186 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_192: calc(36px + (192 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_198: calc(36px + (198 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_204: calc(36px + (204 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_210: calc(36px + (210 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_216: calc(36px + (216 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_222: calc(36px + (222 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_228: calc(36px + (228 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_234: calc(36px + (234 - 36) * ((100vw - 380px) / (1280 - 380)));
    --headline_font_size_240: calc(36px + (240 - 36) * ((100vw - 380px) / (1280 - 380)));
    --body_font_size_8:calc(var(--body_font_size_14) - 6px);
    --body_font_size_9:calc(var(--body_font_size_14) - 5px);
    --body_font_size_10:calc(var(--body_font_size_14) - 4px);
    --body_font_size_11:calc(var(--body_font_size_14) - 3px);
    --body_font_size_12:calc(var(--body_font_size_14) - 2px);
    --body_font_size_13:calc(var(--body_font_size_14) - 1px);
    --body_font_size_14:calc(14px + {{ settings.body_type_size }}px);
    --body_font_size_15:calc(var(--body_font_size_14) + 1px);
    --body_font_size_16:calc(var(--body_font_size_14) + 2px);
    --body_font_size_17:calc(var(--body_font_size_14) + 3px);
    --body_font_size_18:calc(var(--body_font_size_14) + 4px);
    --body_font_size_19:calc(var(--body_font_size_14) + 5px);
    --body_font_size_20:calc(var(--body_font_size_14) + 6px);
    --body_font_size_21:calc(var(--body_font_size_14) + 7px);
    --body_font_size_22:calc(var(--body_font_size_14) + 8px);
    --body_font_size_23:calc(var(--body_font_size_14) + 9px);
    --body_font_size_24:calc(var(--body_font_size_14) + 10px);
    --body_font_size_25:calc(var(--body_font_size_14) + 11px);
    --body_font_size_26:calc(var(--body_font_size_14) + 12px);
    --body_font_size_27:calc(var(--body_font_size_14) + 13px);
    --body_font_size_28:calc(var(--body_font_size_14) + 14px);
    --body_font_size_29:calc(var(--body_font_size_14) + 15px);
    --body_font_size_30:calc(var(--body_font_size_14) + 16px);      
    --label_font_size_10:calc(var(--label_font_size_12) - 2px);
    --label_font_size_11:calc(var(--label_font_size_12) - 1px);
    --label_font_size_12:calc(12px + {{ settings.label_type_size }}px);
    --label_font_size_14:calc(var(--label_font_size_12) + 2px);
    --color_background:{{ settings.color_background }};
    --color_background_section:transparent;
    --color_background_darken_5:{{ settings.color_background | color_darken: 5 }};
    --color_background_modify_5:{{ settings.color_background | color_modify: 'alpha', 0.5 }};
    --color_background_modify_75:{{ settings.color_background | color_modify: 'alpha', 0.75 }};
    --color_background_modify_90:{{ settings.color_background | color_modify: 'alpha', 0.90 }};
    --color_background_outline:{{ settings.color_background }};
    --input_background:var(--color_background);
    --color_overlay:{{ settings.color_background }};
    --color_overlay_opacity: 1;
    --color_border:{{ settings.line_color }};
    --color_border_modify_25:{{ settings.line_color | color_modify: 'alpha', 0.25 }};
    --color_border_modify_5:{{ settings.line_color | color_modify: 'alpha', 0.5 }};
    --color_border_stuck:{{ settings.line_color }};
    --color_accent:{{ settings.color_accent }};
    --color_accent_mix:{{ settings.color_accent | color_mix: settings.color_background, 5 }};  
    --color_accent_modify_25:{{ settings.color_accent | color_modify: 'alpha', 0.25 }};
    --color_accent_modify_5:{{ settings.color_accent | color_modify: 'alpha', 0.5 }};    
    --color_accent_hover:var(--color_background);
    --color_sale:{{ settings.color_sale }};
    --button_border:{{ settings.button_border }}px;
    --button_corner:{{ settings.button_corner }}px;
    --button_primary_border:{{ settings.button_primary_border }};
    --button_primary_background:{{ settings.button_primary_background }};
    --button_primary_text:{{ settings.button_primary_text }};
    --button_primary_text_25:{{ settings.button_primary_text | color_modify: 'alpha', 0.75 }};
    --button_secondary_border:{{ settings.button_secondary_border }};
    --button_secondary_background:{{ settings.button_secondary_background }};
    --button_secondary_background_25:{{ settings.button_secondary_background | color_modify: 'alpha', 0.75 }};
    --button_secondary_text:{{ settings.button_secondary_text }};
    --play_background:var(--color_background);
    --play_arrow:var(--color_text);
    --color_success: {{ settings.success_color }};
    --color_error: {{ settings.error_color }};
    --color_menu_text:{{ settings.color_menu_text }};
    --color_title_text:{{ settings.color_title_text }};
    --color_title_text_mix:{{ settings.color_title_text | color_mix: settings.color_background, 5 }};
    --color_title_text_modify_5:{{ settings.color_title_text | color_modify: 'alpha', 0.05 }};
    --color_title_text_modify_75:{{ settings.color_title_text | color_modify: 'alpha', 0.75 }};
    --color_text:{{ settings.color_text }};
    --color_text_modify_5:{{ settings.color_text | color_modify: 'alpha', 0.5 }};
    --color_text_modify_75:{{ settings.color_text | color_modify: 'alpha', 0.75 }};
    --color_text_hover:{{ settings.color_text_hover }};
    --color_label:{{ settings.color_label }};
    --color_rating:{{ settings.rating_color }};
    --color_rating_background:{{ settings.rating_background_color }};
    --color-body-text: var(--color_text);
    --color-body: var(--color_background);
    --color-bg: var(--color_background);
    --marquee_gap: 10px;
    --marquee_speed: 10s;
    --hotspot_color_rgba: {{ settings.color_background | color_modify: 'alpha', 1 }};
    --hotspot_color_rgba_0: {{ settings.color_background | color_modify: 'alpha', 0 }};
    --hotspot_color_rgba_70: {{ settings.color_background | color_modify: 'alpha', 0.7 }};
    --slider_color:{{ settings.color_accent }};
    --slider_color_modify_25:{{ settings.color_accent | color_modify: 'alpha', 0.25 }};
    --grid-mobile: 20px;
    --grid-desktop: 20px;
    --scrollbar:20px;
    --image-rounded:0px;
    @media (pointer:coarse) {
      --scrollbar:0px;      
    }
    --stars:12;
    --star_spacing: 2;
    --star_size: calc(var(--stars) * 1.5);
    --header_gutter: 20px;    
    --sticky_menu: 3014;
    --index_product_alert: 3016;
    --index_header_overlay: 2005;
    --index_contactbar: 2006;
    --index_mobilenav: 3016;
    --index_meganav: 3009;
    --index_header_navmenu: 2007;
    --index_header_minicart: 3017;
    --index_modal: 2502;
    --quickshop: 2503;
    --ease_out_quad: ease-in-out;
    --ease_out_cubic: ease-in-out;
  }
  @media only screen and (min-width: 1280px) {
    :root {
      --headline_font_size_12: 12px;
      --headline_font_size_14: 14px;
      --headline_font_size_16: 16px;
      --headline_font_size_18: 18px;
      --headline_font_size_24: 24px;
      --headline_font_size_30: 30px;
      --headline_font_size_36: 36px;
      --headline_font_size_42: 42px;
      --headline_font_size_48: 48px;
      --headline_font_size_54: 54px;
      --headline_font_size_60: 60px;
      --headline_font_size_66: 66px;
      --headline_font_size_72: 72px;
      --headline_font_size_78: 78px;
      --headline_font_size_84: 84px;
      --headline_font_size_90: 90px;
      --headline_font_size_96: 96px;
    }
  }
</style>