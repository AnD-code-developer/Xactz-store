{%- liquid
  assign show_announcement = false
  assign layout_announcement = section.settings.layout
  assign enable_announcement = false
  assign full_width = section.settings.full_width
  assign blocks_size = section.blocks.size  
  if layout_announcement == 'slideshow'
    assign blocks_size = 0
    for block in section.blocks
      if block.settings.announcement_text_mobile != blank or block.settings.announcement_text_desktop != blank
        assign blocks_size = blocks_size | plus: 1
      endif
    endfor
  endif
  if blocks_size > 0
    assign enable_announcement = true
  endif
  assign homepage_only = section.settings.announcement_home_only
  if enable_announcement and homepage_only
    if request.page_type == 'index'
      assign show_announcement = true
    endif
  elsif enable_announcement
    assign show_announcement = true
  endif
-%}
{% if show_announcement and enable_announcement %}
  {%- liquid
    assign text_color = false
    if section.settings.text_color.alpha != 0.0 and section.settings.text_color.alpha != blank
      assign text_color = true
      assign text_color_section = section.settings.text_color
    endif    
    assign text_color_hover = false
    if section.settings.text_color_hover.alpha != 0.0 and section.settings.text_color_hover.alpha != blank
      assign text_color_hover = true
      assign text_color_hover_section = section.settings.text_color_hover
    endif
    assign line_color = false
    if section.settings.line_color.alpha != 0.0 and section.settings.line_color.alpha != blank
      assign line_color = true
      assign line_color_section = section.settings.line_color
    endif
    assign background_color = false
    if section.settings.background_color.alpha != 0.0 and section.settings.background_color != blank
      assign background_color = true
      assign background_color_section = section.settings.background_color
    endif  
    if section.settings.background_gradient != blank
      assign background_color = true
      assign background_color_section = section.settings.background_gradient
    endif
  -%}
  {% style %}
    .announcement-bar-{{ section.id }} {
      {%- if text_color -%}
      --color_text: {{ text_color_section }};
      --color_title_text:{{ text_color_section }};
      {%- endif -%}
      {%- if text_color_hover -%}
      --color_text_hover:{{ text_color_hover_section }};
      {%- endif -%}
      {%- if line_color -%}
      --color_border:{{ line_color_section }};
      {%- endif -%}
      {%- if background_color -%}
      --color_background: {{ background_color_section }};
      {%- if layout_announcement == 'marquee' -%}--color_background_section: {{ background_color_section }};{%- endif -%}
      {%- endif -%}
      --section_padding: {{ section.settings.announcement_padding }}px;
      --marquee_gap: {{ section.settings.marquee_gap }}px;
      --marquee_speed: {{ section.settings.marquee_speed }}s;
    }    
  {% endstyle %}
  {%- liquid
    if layout_announcement == 'marquee'
      assign marquee_seperator = blank
      if section.settings.marquee_seperator != 'none' or section.settings.icon_custom != blank
        capture marquee_seperator
          if section.settings.marquee_seperator contains 'punctuation-' and section.settings.icon_custom == blank
            case section.settings.marquee_seperator
              when 'punctuation-bullet'
                echo '•'
              when 'punctuation-check'
                echo '✔'
              when 'punctuation-dash'
                echo '〰'
              when 'punctuation-heart'
                echo '❤'
              when 'punctuation-pipe'
                echo '⁞'
              when 'punctuation-star'
                echo '★'
              when 'punctuation-x'
                echo '✖'
            endcase
          else
            render 'icons', block: section, id: section.settings.marquee_seperator, size: section.settings.icon_size, custom: section.settings.icon_custom
          endif
        endcapture
      endif
      assign size_mobile = 0
      capture items_mobile                
        for block in section.blocks
          if block.settings.announcement_text_mobile != blank
            assign announcement_text_mobile = block.settings.announcement_text_mobile
          elsif block.settings.announcement_text_desktop != blank
            assign announcement_text_mobile = block.settings.announcement_text_desktop
          endif
          echo '<span class="marquee-item dynamic-block-'
          echo block.id
          echo ' body-font-'
          echo block.settings.announcement_text_size
          echo '">'
            echo announcement_text_mobile
          echo '</span>'
          if marquee_seperator != blank
            echo '<span class="relative marquee-item">'
              render 'image.sizes', size: section.settings.icon_size
              echo marquee_seperator
            echo '</span>'
          endif
          if announcement_text_mobile != blank
            assign size_mobile = size_mobile | plus: 1
          endif
        endfor 
      endcapture    
      capture final_items_mobile
        if size_mobile > 0
          echo '<div class="marquee-items mobile'
          if section.settings.marquee_direction == 'reverse'
            echo ' reverse'
          endif
          echo '">'
            echo items_mobile
          echo '</div>'
          assign total_mobile = 10
          for i in (1..total_mobile)
            echo '<div class="marquee-items mobile'
            if section.settings.marquee_direction == 'reverse'
              echo ' reverse'
            endif
            echo '" aria-hidden="true">'
              echo items_mobile
            echo '</div>'
          endfor
        endif
      endcapture
      assign size_desktop = 0         
      capture items_desktop                
        for block in section.blocks
          if block.settings.announcement_text_desktop != blank
            assign announcement_text_desktop = block.settings.announcement_text_desktop
          elsif block.settings.announcement_text_mobile != blank
            assign announcement_text_desktop = block.settings.announcement_text_mobile
          endif
          echo '<span class="marquee-item dynamic-block-'
          echo block.id
          echo ' body-font-'
          echo block.settings.announcement_text_size
          echo '">'
            echo announcement_text_desktop
          echo '</span>'
          if marquee_seperator != blank
            echo '<span class="relative marquee-item">'
              render 'image.sizes', size: section.settings.icon_size
              echo marquee_seperator
            echo '</span>'
          endif
          if announcement_text_desktop != blank
            assign size_desktop = size_desktop | plus: 1    
          endif
        endfor 
      endcapture    
      capture final_items_desktop
        if size_desktop > 0
          echo '<div class="marquee-items desktop'
          if section.settings.marquee_direction == 'reverse'
            echo ' reverse'
          endif
          echo '">'
            echo items_desktop
          echo '</div>'
          case size_desktop
            when 1
              assign total_desktop = 30
            when 2
              assign total_desktop = 15
            else
              assign total_desktop = 10
          endcase
          for i in (1..total_desktop)
            echo '<div class="marquee-items desktop'
            if section.settings.marquee_direction == 'reverse'
              echo ' reverse'
            endif
            echo '" aria-hidden="true">'
              echo items_desktop
            echo '</div>'
          endfor
        endif
      endcapture
    endif
  -%}
  {% if size_mobile > 0 or size_desktop > 0 or layout_announcement == 'slideshow' and blocks_size > 0 %}
    <div class="dynamic-section announcement-marquee announcement-bar-{{ section.id }} body-font-{{ section.settings.icon_size }} section-background">
      <section data-section-id="{{ section.id }}"
               data-section-type="slider-{{ section.id }}"
               {% if layout_announcement == 'slideshow' and blocks_size > 1 %}
               data-url="{{ 'script.slider.js' | asset_url }}" 
               data-slider-use="true"
               data-slider-breakpoint="large"
               data-should-autoplay="{{ section.settings.should_autoplay }}"
               data-autoplay="{{ section.settings.slide_autoplay | times: 1000 }}"
               data-full-width="false"
               data-slideshow
               {% endif %}
               class="{% if layout_announcement == 'slideshow' %}slideshow{% else %}marquee{% if full_width %} section-skip{% endif %}{% endif %}">    
          {% if layout_announcement == 'slideshow' %}
            {% if blocks_size > 1 %}<div class="slideshow-container{% if section.settings.padding_border %} section-border{% endif %}"><div class="slider large slider-loading{% if section.settings.should_autoplay %} active{% endif %}">{% endif %}
              {%- liquid
                assign forloop_first = true
                assign forloop_index = 0
              -%}
              {% for block in section.blocks %}
                {% if block.settings.announcement_text_mobile != blank or block.settings.announcement_text_desktop != blank %} 
                  {%- liquid
                    assign use_style = false
                    assign text_color = false
                    if block.settings.announcement_color.alpha != 0.0 and block.settings.announcement_color.alpha != blank
                      assign use_style = true
                      assign text_color = true
                      assign text_color_section = block.settings.announcement_color
                    endif
                    assign text_color_hover = false
                    if block.settings.announcement_color_hover.alpha != 0.0 and block.settings.announcement_color_hover.alpha != blank
                      assign use_style = true
                      assign text_color_hover = true
                      assign text_color_hover_section = block.settings.announcement_color_hover
                    endif                       
                  -%}
                  {% if use_style %}
                    {% style %}   
                        .dynamic-block-{{ block.id }} {
                          {%- if text_color -%}
                          --color_text: {{ text_color_section }};
                          {%- endif -%}
                          {%- if text_color_hover -%}
                          --color_text_hover:{{ text_color_hover_section }};
                          {%- endif -%}
                        }
                    {% endstyle %}
                  {% endif %}
                  {%- liquid
                    assign forloop_index = forloop_index | plus: 1
                    capture previous
                      if forloop_first
                        echo section.id
                        echo '-'
                        echo blocks_size
                      else
                        echo section.id
                        echo '-'
                        echo forloop_index | minus: 1
                      endif
                    endcapture
                    capture next
                      if forloop.last
                        echo section.id
                        echo '-1'
                      else
                        echo section.id
                        echo '-'
                        echo forloop_index | plus: 1
                      endif
                    endcapture
                  -%}
                  <article id="slide-{{ section.id }}-{{ forloop_index }}" class="dynamic-block-{{ block.id }} body-font-{{ block.settings.announcement_text_size }} text-center{% if blocks_size > 1 %} slide{% if forloop_index == 1 %} active{% else %}{% if forloop_index == 2 %} next{% endif %}{% if forloop.last %} previous{% endif %}{% endif %}{% endif %} slideshow-slide{% if blocks_size == 1 %} single{% endif %} section-layout{% if full_width %} full_width skip{% endif %}" 
                           data-slideshow-slide 
                           data-slide="{{ forloop_index }}"  
                           data-previous="{{ previous | split: '-' | last }}"  
                           data-next="{{ next | split: '-' | last }}" {{ block.shopify_attributes }}>
                    <div class="slide-container announcement">                  
                      {%- liquid
                        assign announcement_text_mobile = block.settings.announcement_text_mobile
                        assign announcement_text_desktop = block.settings.announcement_text_desktop
                        if block.settings.announcement_text_mobile != blank
                          echo '<span class="announcement-bar-text'
                          if announcement_text_desktop != blank
                            echo ' mobile'
                          endif
                          echo '">'
                            echo announcement_text_mobile
                          echo '</span>'
                        endif
                        if block.settings.announcement_text_desktop != blank
                          echo '<span class="announcement-bar-text'
                          if announcement_text_mobile != blank
                            echo ' desktop'
                          endif
                          echo '">'
                            echo announcement_text_desktop
                          echo '</span>'
                        endif
                      -%}
                      {% if blocks_size > 1 %}
                        <div class="slideshow-buttons slideshow-buttons-announcement">
                          <button value="slide-{{ previous }}" 
                                  data-index="{{ previous | split: '-' | last }}"
                                  data-type="previous"
                                  class="slider-button previous link-group keyed-skip keyed-link"
                                  title="{{ 'pagination.prev' | t }}" 
                                  aria-label="{{ 'pagination.prev' | t }}"
                                  {% unless forloop_first %}tabindex="-1" aria-hidden="true"{% endunless %}><span class="link-arrow-left">{%- render 'icons.theme', id: 'arrow-long-l', size: 18 -%}</span></button>
                          <button value="slide-{{ next }}" 
                                  data-index="{{ next | split: '-' | last }}"
                                  data-type="next"
                                  class="slider-button next link-group keyed-skip keyed-link"
                                  title="{{ 'pagination.next' | t }}" 
                                  aria-label="{{ 'pagination.next' | t }}"
                                  {% unless forloop_first %}tabindex="-1" aria-hidden="true"{% endunless %}><span class="link-arrow">{%- render 'icons.theme', id: 'arrow-long-r', size: 18 -%}</span></button>
                        </div>
                      {% endif %}
                    </div>
                  </article>
                  {%- assign forloop_first = false -%}
                {% endif %}
              {% endfor %}
            {% if blocks_size > 1 %}</div></div>{% endif %} 
          {% else %}
            <article class="marquee-container{% if section.settings.padding_border %} section-border{% endif %} section-background">
              {% for block in section.blocks %}
                {% style %}    
                  {%- liquid
                    assign use_style = false
                    assign text_color = false
                    if block.settings.announcement_color.alpha != 0.0 and block.settings.announcement_color.alpha != blank
                      assign use_style = true
                      assign text_color = true
                      assign text_color_block = block.settings.announcement_color
                    endif
                    assign text_color_hover = false
                    if block.settings.announcement_color_hover.alpha != 0.0 and block.settings.announcement_color_hover.alpha != blank
                      assign use_style = true
                      assign text_color_hover = true
                      assign text_color_hover_block = block.settings.announcement_color_hover
                    endif                       
                  -%}    
                  {% if use_style %}
                    .dynamic-block-{{ block.id }} {
                      {%- if text_color -%}
                      --color_text: {{ text_color_block }};
                      --color_title_text:{{ text_color_block }};
                      {%- endif -%}
                      {%- if text_color_hover -%}
                      --color_text_hover:{{ text_color_hover_block }};
                      {%- endif -%}
                    }                  
                  {% endif %}
                {% endstyle %}
              {% endfor %}
              {%- liquid
                echo final_items_mobile
                echo final_items_desktop
              -%}
            </article>
          {% endif %}    
      </section>
    </div>
  {% endif %}
{% endif %}
