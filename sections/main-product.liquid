{%- liquid
  assign section_blocks_size = 0
  assign form_text = 'product.add_to_cart.available' | t | escape
  assign variant = product.selected_or_first_available_variant
  assign product_quantity = false
  assign product_variants = false
  for block in section.blocks
    case block.type
      when 'contact'
        capture contact_message
          render 'block.contact', section: block, block: block, section_id: section.id, is_product: true, is_message: true
        endcapture
      when 'variants'
        assign product_variants = true
        assign variant_enable = block.settings.variant_enable
      when 'quantity'
        assign product_quantity = true
       when 'form'
        assign enable_cart_redirection = block.settings.enable_cart_redirection
        assign show_payment_button = block.settings.show_payment_button
      else
        assign section_blocks_size = 1
    endcase
  endfor
  capture added_to_cart_link
    render 'icons.theme', id: 'checkmark'
    echo 'product.added_to_cart_link_html' | t
  endcapture
  assign gallery_position = section.settings.gallery_position_desktop
  assign gallery_layout = section.settings.gallery_layout
  case gallery_layout
    when 'list'
    when 'gallery-right'
    when 'gallery-below'
    when 'gallery-left'
  else
    assign gallery_layout = 'list'
  endcase
  if section.settings.enable_lazy
    assign lazy = 'lazy'
  else
    assign lazy = 'eager'
  endif
  assign text_color = false
  if section.settings.text_color.alpha != 0.0 and section.settings.text_color.alpha != blank
    assign text_color = true
    assign text_color_section = section.settings.text_color
  endif
  assign text_color_hover = false
  if section.settings.text_color_hover.alpha != 0.0 and section.settings.text_color_hover.alpha != blank
    assign text_color_hover = true
    assign text_color_hover_section = section.settings.text_color_hover
  endif
  assign button_color = false
  if section.settings.button_color.alpha != 0.0 and section.settings.button_color.alpha != blank
    assign button_color = true
    assign button_color_section = section.settings.button_color
  endif
  assign line_color = false
  if section.settings.line_color.alpha != 0.0 and section.settings.line_color.alpha != blank
    assign line_color = true
    assign line_color_section = section.settings.line_color
  endif
  assign slider_color = false
  if section.settings.slider_color.alpha != 0.0 and section.settings.slider_color.alpha != blank
    assign slider_color = true
    assign slider_color_section = section.settings.slider_color
  endif
  assign background_color = false
  if section.settings.background_color.alpha != 0.0 and section.settings.background_color.alpha != blank
    assign background_color = true
    assign background_color_section = section.settings.background_color
  endif
  if section.settings.background_gradient != blank
    assign background_color = true
    assign background_color_section = section.settings.background_gradient
  endif
-%}
{% style %}
  .dynamic-section-{{ section.id }} {
    --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
    --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
    --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
    --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
    --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
    --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px;
    {%- if text_color -%}
    --color_title_text:{{ text_color_section }};
    --color_text:{{ text_color_section }};
    --color_text_modify_5:{{ text_color_section | color_modify: 'alpha', 0.5 }};
    --color_text_modify_75:{{ text_color_section | color_modify: 'alpha', 0.75 }};
    --color_title_text_modify_75:{{ text_color_section | color_modify: 'alpha', 0.75 }};
    --color_label:{{ text_color_section }};
    --color_accent:{{ text_color_section }};
    --color_accent_modify_25:{{ text_color_section | color_modify: 'alpha', 0.25 }};
    --color_error:{{ text_color_section }};
    --color_success:{{ text_color_section }};
    --color_rating:{{ text_color_section }};
    --color_rating_background:{{ text_color_section | color_modify: 'alpha', 0.35 }};
    --color_sale:{{ text_color_section }};
    --color_accent:{{ text_color_section }};
    --button_primary_background:{{ text_color_section }};
    --button_primary_border:{{ text_color_section }};
    --button_secondary_border:{{ text_color_section }};
    --button_secondary_text:{{ text_color_section }};      
    {%- endif -%}
    {%- if text_color_hover -%}
    --color_text_hover:{{ text_color_hover_section }};
    {%- endif -%}
    {%- if line_color -%}
    --color_border:{{ line_color_section }};
    --color_border_modify_5:{{ line_color_section | color_modify: 'alpha', 0.5 }};
    {%- endif -%}
    {%- if button_color -%}  
      --button_primary_text:{{ button_color_section }};
      --button_secondary_background:{{ button_color_section }};
      --color_accent_hover:{{ button_color_section }};
    {%- endif -%}  
    {%- if slider_color -%}
      --slider_color:{{ slider_color_section }};
      --slider_color_modify_25:{{ slider_color_section | color_modify: 'alpha', 0.25 }};
    {%- endif -%}  
    {%- if background_color -%}
      --color_background_outline:{{ section.settings.background_color }};
      --color_background:{{ background_color_section }};
      --color_background_section:{{ background_color_section }};
    {%- endif -%}
  }
{% endstyle %}
<div class="dynamic-section dynamic-section-{{ section.id }} section-background">
  <section data-product-id="{{ product.id }}" 
           data-section-id="{{ section.id }}" 
           data-onboarding="false"
           data-section-type="product-{{ section.id }}"
           data-url="{{ 'script.product.js' | asset_url }}">
    <div class="product-page container-inner section-padding-top section-padding-bottom section-margin-bottom{% if section.settings.padding_border %} section-border{% endif %}">
      {%- liquid
        capture product_media
          assign gallery_layout = gallery_layout | default: 'list'
          assign selected_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media
          render 'product.images', product: product, section: section, selected_media: selected_media, layout: gallery_layout, insta_filter: section.settings.insta_filter, play: section.settings.gallery_video_autoplay, loop: section.settings.gallery_video_looping, lazy: lazy
        endcapture
      -%}
      <div class="product product-wrapper" data-product-gallery-position="{{ gallery_position }}">        
        {%- liquid
          if gallery_position == 'left'
            echo product_media
          endif
        -%}        
        <div id="product-form-{{ section.id }}" class="product-details{% unless section_blocks_size > 0 %} product-details-empty{% endunless %}" data-product-details data-product-info data-href="{% unless settings.seo %}{{ product.url | within: collection }}{% else %}{{ product.url }}{% endunless %}">
          {%- liquid
            echo contact_message
            capture form_classes
              echo 'product-form-inline content-details-sticky'
              if show_payment_button
                echo ' product-form-payment-buttons'
              endif
            endcapture
            form 'product', product, class: form_classes, data-product-form-inline: ''
              for block in section.blocks
                case block.type
                  when 'breadcrumbs'
                    echo '<div class="content-block" '
                    echo block.shopify_attributes
                    echo '>'
                      render 'breadcrumbs', breadcrumbs_size: block.settings.title_size
                    echo '</div>'
                  when 'vendor'
                    render 'product.vendor', product: product, block: block
                  when 'title'
                    render 'product.title', product: product, block: block
                  when 'sku'
                    render 'product.sku', product: product, block: block
                  when 'rating'
                    render 'product.rating', product: product, block: block, product_page: true
                  when 'price'
                    render 'product.price', product: product, block: block, parent_type: 'main-product'
                  when 'inventory'
                    render 'product.inventory', product: product, variant: variant, block: block
                  when 'variants'
                    render 'product.options', product: product, block: block, size: 40, type: variant_enable, hidden: false, lazy: lazy                    
                  when 'quantity'
                    echo '<div class="content-block" '
                    echo block.shopify_attributes
                    echo '>'
                      render 'product.quantity', product: product, variant: variant, block: block, is_product: true
                    echo '</div>'                    
                  when 'form'             
                    assign meta = 'enable_custom'
                    assign metafirst = settings[meta] | split:':' | last | split:'.' | first
                    assign metalast = settings[meta] | split:':' | last | split:'.' | last
                    assign metatext = product.metafields[metafirst][metalast]
                    if settings.enable_custom != blank and metatext.value != blank
                      assign form_text = metatext.value
                    endif
                    if block.settings.show_gift_card_recipient and product.gift_card?
                      render 'product.gift', product: product, block: block, form: form
                    endif
                    render 'product.form', product: product, variant: variant, block: block, form: form, form_text: form_text, show_payment_button: show_payment_button, background_color: background_color
                  when 'shoppay'
                    render 'product.shoppay', product: product, form: form, block: block
                  when 'payments'
                    render 'block.payments', block: block
                  when 'shipping_message'
                    if settings.shipping_amount != blank
                      echo '<shipping-message class="content-block variant_metafields" '
                      echo block.shopify_attributes
                      echo '>'
                      render 'cart.shipping.message'
                      echo '</shipping-message>'
                    endif                    
                  when 'store'
                    echo '<div class="surface-pick-up slider-v" data-surface-pick-up data-base-url="'
                    echo shop.url | append: routes.root_url
                    echo '" data-geo="'
                    echo block.settings.geo
                    echo ' '
                    echo block.shopify_attributes
                    echo '"></div>'
                  when 'description'
                    render 'product.description', product: product, block: block          
                  when 'complementary'
                    render 'product.complementary', block: block, product: product, lazy: lazy
                  when 'social'
                    echo '<div class="content-block'
                    if block.settings.center_social
                      echo ' text-center'
                    endif
                    echo'">'
                      render 'block.social.share', block: block
                    echo '</div>'
                  when '@app'
                    echo '<div class="content-block '
                    echo block.shopify_attributes
                    echo '">'
                      render block, block: block
                    echo '</div>'
                  when 'icon'
                    echo '<div class="content-block" '
                    echo block.shopify_attributes
                    echo '>'
                      render 'block.icon', section: block, block: block, is_product: true, lazy: lazy
                    echo '</div>'
                  when 'button'
                    render 'product.button', block: block
                  when 'meta'
                    render 'product.meta', block: block
                  when 'page'
                    render 'product.page', block: block
                  when 'properties'
                    render 'product.properties', block: block
                  when 'image'
                    render 'product.image', product: product, block: block, lazy: lazy
                  when 'contact'
                    render 'block.contact', product: product, section: block, block: block, section_id: section.id, is_product: true, is_form: true
                  when 'liquid'
                    render 'block.code', block: block
                  when 'divider'
                    echo '<div class="content-block divider divider-top-'
                    echo block.settings.padding_top
                    echo ' divider-bottom-'
                    echo block.settings.padding_bottom
                    echo '" '
                    echo block.shopify_attributes
                    echo '><hr/></div>'
                endcase
              endfor
              unless product_quantity
                echo '<input class="quantity" type="hidden" min="1" max="1" name="quantity" value="1" data-quantity-input data-quantity-hidden />'
              endunless
              if product_variants == false
                echo '<div class="hidden">'
                  render 'product.options', product: product, type: 'select', hidden: true
                echo '</div>'
              endif
              render 'product.json', product: product, form_text: form_text, enable_cart_redirection: enable_cart_redirection, variant_enable: variant_enable, added_to_cart_link: added_to_cart_link, use_history: true, gallery_zoom: section.settings.gallery_zoom, gallery_video_autoplay: section.settings.gallery_video_autoplay, gallery_video_looping: section.settings.gallery_video_looping, template: 'page'
            endform
          -%}
        </div>
        {%- liquid
          if gallery_position == 'right'
            echo product_media
          endif
        -%}
      </div>
    </div>
  </section>
</div>
