{% if section.blocks.size > 0 %}
  {%- liquid
    assign full_width = section.settings.full_width
    assign blocks_size_mobile = 0
    assign blocks_size_desktop = 0
    for block in section.blocks  
      assign blocks_size_mobile = blocks_size_mobile | plus: block.settings.content_width_mobile  
      assign blocks_size_desktop = blocks_size_desktop | plus: block.settings.content_width_desktop
    endfor
    assign enable_slider = false  
    assign enable_slider_mobile = section.settings.enable_slider_mobile
    assign enable_slider_desktop = section.settings.enable_slider_desktop
    if enable_slider_mobile or enable_slider_desktop
      if enable_slider_mobile and enable_slider_desktop == false
        if blocks_size_mobile > 4
          assign enable_slider = true
          assign enable_slider_size = 'small'
        endif
      elsif enable_slider_mobile == false and enable_slider_desktop
        if blocks_size_desktop > 6
          assign enable_slider = true
          assign enable_slider_size = 'large-only'
        endif  
      else
        if blocks_size_mobile > 4 and blocks_size_desktop > 6
          assign enable_slider = true
          assign enable_slider_size = 'large'
        elsif blocks_size_mobile < 4 and blocks_size_desktop > 6
          assign enable_slider = true
          assign enable_slider_size = 'large-only'
        elsif blocks_size_mobile > 4 and blocks_size_desktop < 6
          assign enable_slider = true
          assign enable_slider_size = 'small'
        endif
      endif          
    endif  
    if section.settings.enable_lazy
      assign lazy = 'lazy'
    else
      assign lazy = 'eager'
    endif
    assign slider_color = false
    if section.settings.slider_color.alpha != 0.0 and section.settings.slider_color.alpha != blank
      assign slider_color = true
      assign slider_color_section = section.settings.slider_color
    endif
    assign background_color = false
    if section.settings.background_color.alpha != 0.0 and section.settings.background_color.alpha != blank
      assign background_color = true
      assign background_color_section = section.settings.background_color
    endif
    if section.settings.background_gradient != blank
      assign background_color = true
      assign background_color_section = section.settings.background_gradient
    endif
  -%}  
  {% style %}
    .dynamic-section-{{ section.id }} {
      --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
      --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
      --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
      --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
      --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
      --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px;      
      --content_width_mobile: 100%;
      --content_width_desktop: 100%;    
      --grid-mobile: {{ section.settings.padding_grid_mobile }}px;
      --grid-desktop: {{ section.settings.padding_grid_desktop }}px;  
      {%- if slider_color -%}
      --slider_color:{{ slider_color_section }};
      --slider_color_modify_25:{{ slider_color_section | color_modify: 'alpha', 0.25 }};
      {%- endif -%}
      {%- if background_color -%}
        --color_background_outline:{{ section.settings.background_color }};
        --color_background:{{ background_color_section }};
        --color_background_section:{{ background_color_section }};
      {%- endif -%}
    }    
  {% endstyle %}  
  <div class="dynamic-section dynamic-section-{{ section.id }}{% if full_width %} section-background{% endif %}">
    <section data-section-id="{{ section.id }}" 
             {% if enable_slider %}
               data-section-type="slider-{{ section.id }}"
               data-url="{{ 'script.slider.js' | asset_url }}"
               data-slider-use="true"
               data-slider-breakpoint="{{ enable_slider_size }}"
             {% else %}
               data-section-type="image-with-text-{{ section.id }}"
               data-slider-breakpoint="none"
             {% endif %}
             class="relative">  
      <div class="section-layout-card-wrapper section-padding-top section-padding-bottom section-margin-bottom{% if section.settings.padding_border %} section-border{% endif %}{% unless full_width %} section-background{% endunless %}">
        <div class="{% if enable_slider %}slider-container slider-padding {% endif %}section-card-wrapper section-card-wrapper-{{ section.settings.layout_x }}">          
          <div class="{% if enable_slider %}slider {{ enable_slider_size }} scroll-bar-h {% endif %}{% unless enable_slider_size == 'large' %}container-inner{% endunless %}">
            <div class="dynamic-block-{{ section.id }} section-card-rows skip">
              {%- liquid
                assign mn = 0
                assign dn = 0
                assign mr = 1
                assign dr = 1              
                assign mobile_class = 'mobile-first'
                assign desktop_class = 'desktop-first'
                assign slider_mobile = section.settings.enable_slider_mobile
                assign slider_desktop = section.settings.enable_slider_desktop
                assign rows_mobile_padding = section.settings.padding_grid_mobile
                assign rows_desktop_padding = section.settings.padding_grid_desktop
              -%}
              {% for block in section.blocks %}
                {%- liquid
                  assign rows_mobile = block.settings.content_width_mobile
                  assign rows_desktop = block.settings.content_width_desktop
                  assign mn = mn | plus: rows_mobile
                  assign dn = dn | plus: rows_desktop
                  if mn == 4
                    assign mobile_class = 'mobile-last'
                  else
                    unless forloop.first
                      assign mobile_class = 'mobile-middle'
                    endunless
                  endif              
                  if mn > 4
                    assign mn = 0 | plus: rows_mobile
                    assign mr = mr | plus: 1
                    assign mobile_class = 'mobile-first'                
                  endif             
                  if dn == 6
                    assign desktop_class = 'desktop-last'
                  else
                    unless forloop.first
                      assign desktop_class = 'desktop-middle'
                    endunless
                  endif
                  if dn > 6
                    assign dn = 0 | plus: rows_desktop
                    assign dr = dr | plus: 1
                    assign desktop_class = 'desktop-first'                
                  endif
                  assign class = mobile_class | append: ' ' | append: desktop_class
                -%}                
                <article class="{% unless section.settings.image_height == 'original' %}height-padding-{{ section.settings.image_height }} gallery {% endunless %}dynamic-block-{{ block.id }} section-card no-background{% if block.settings.include_border %} section-card-borders{% unless section.settings.image_height == 'original' %}-image{% endunless %}{% endif %} slide section-card-{{ section.settings.layout_y }} mobile-rows-of-{{ rows_mobile }} desktop-rows-of-{{ rows_desktop }} {{ class }} section-layout{% if settings.animation_enable %} animated-item animated-delay-{{ forloop.index0 }}{% endif %}" {{ block.shopify_attributes }}>
                  {%- liquid
                    unless section.settings.image_height == 'original'
                      assign gallery = true
                    else
                      assign gallery = false
                    endunless
                    case rows_mobile
                      when 1
                        assign mobile_size = 178
                      when 2
                        assign mobile_size = 355
                      when 3
                        assign mobile_size = 532
                      when 4
                        assign mobile_size = 710
                    endcase
                    if slider_mobile
                      assign mobile_divide = 0
                    else
                      assign mobile_divide = 1
                    endif
                    capture mobile_sizes
                      echo 'calc(((100dvw - 40px) * ('
                      echo rows_mobile
                      echo ' / 4)) - ('
                      if rows_mobile < 4
                        echo rows_mobile_padding
                      else
                        echo 0
                      endif
                      echo 'px * ('
                      echo mobile_divide
                      echo ' / 1)))'
                    endcapture
                    case rows_desktop
                      when 1
                        assign desktop_size = 207
                      when 2
                        assign desktop_size = 413
                      when 3
                        assign desktop_size = 620
                      when 4
                        assign desktop_size = 827
                      when 5
                        assign desktop_size = 1033
                      when 6
                        assign desktop_size = 1240
                    endcase
                    if slider_desktop
                      assign desktop_divide = 0
                    else
                      assign desktop_divide = 1
                    endif
                    capture desktop_sizes
                      echo 'calc(((100dvw - 40px) * ('
                      echo rows_desktop
                      echo ' / 6)) - ('
                      if rows_desktop < 6
                        echo rows_desktop_padding
                      else
                        echo 0
                      endif
                      echo 'px * ('
                      echo desktop_divide
                      echo ' / 1)))'
                    endcapture
                    assign mobile_size_onboard = mobile_size
                    if mobile_size > desktop_size
                      assign desktop_size = mobile_size
                    endif
                    if block.settings.image_desktop == blank
                      assign mobile_size = desktop_size
                    endif
                    capture mobile_sizes_final
                      echo '(min-width: 1280px) '
                      if block.settings.image_desktop == blank
                        echo desktop_size
                      else
                        echo mobile_size
                      endif
                      echo 'px, (min-width: 768px) '
                      echo desktop_sizes
                      echo ', '
                      echo mobile_sizes
                    endcapture                   
                    capture desktop_sizes_final
                      echo '(min-width: 1280px) '
                      echo desktop_size
                      echo 'px, (min-width: 768px) '
                      echo desktop_sizes
                      echo ', '
                      echo mobile_sizes
                    endcapture                
                    if block.settings.image_desktop != blank or block.settings.image_mobile != blank
                      if block.settings.link != blank
                        echo '<a href="'
                        echo block.settings.link
                        echo '" class="block-element section-figure"'
                        if block.settings.link_seo != blank
                          echo 'title="'
                          echo block.settings.link_seo | escape
                          echo '" class="block"'
                        endif
                        echo '>'
                      else
                        echo '<div class="block-element section-figure">'
                      endif 
                      render 'block.images', section: section, block: block, mobile_size: mobile_size, mobile_sizes: mobile_sizes_final, desktop_size: desktop_size, desktop_sizes: desktop_sizes_final, gallery: gallery, padding: section.settings.image_height, lazy: lazy, forloop: forloop
                      if block.settings.link != blank
                        echo '</a>'
                      else
                        echo '</div>'
                      endif
                    else
                      echo '<figure class="section-layout-wrapper'
                      if gallery
                        echo ' gallery'
                      endif
                      echo '">'
                        assign title = 'hero-apparel-'
                        assign index = forloop.index0 | modulo: 3 | plus: 1
                        echo title | append: index | placeholder_svg_tag: 'placeholder-svg'
                      echo '</figure>'
                    endif
                    render 'image.sizes', mobile_size: mobile_size_onboard, desktop_size: desktop_size, padding_mobile: section.settings.image_height, padding_desktop: section.settings.image_height, new_size_mobile: 768, new_size_desktop: 1240
                  -%}
                </article>
              {% endfor %}
            </div>
          </div>          
        </div>    
      </div>
    </section>
  </div>
{% endif %}
