{%- liquid
  assign text_color_customer = false
  if section.settings.text_color_customer.alpha != 0.0 and section.settings.text_color_customer.alpha != blank
    assign text_color_customer = true
    assign text_color_customer_section = section.settings.text_color_customer
  endif
  assign text_color_hover_customer = false
  if section.settings.text_color_hover_customer.alpha != 0.0 and section.settings.text_color_hover_customer.alpha != blank
    assign text_color_hover_customer = true
    assign text_color_hover_customer_section = section.settings.text_color_hover_customer
  endif
  assign line_color_customer = false
  if section.settings.line_color_customer.alpha != 0.0 and section.settings.line_color_customer.alpha != blank
    assign line_color_customer = true
    assign line_color_customer_section = section.settings.line_color_customer
  endif
  assign background_color_customer = false
  if section.settings.background_color_customer.alpha != 0.0 and section.settings.background_color_customer != blank
    assign background_color_customer = true
    assign background_color_customer_section = section.settings.background_color_customer
  endif  
  if section.settings.background_gradient_customer != blank
    assign background_color_customer = true
    assign background_color_customer_section = section.settings.background_gradient_customer
  endif
  assign text_color_navigation = false
  if section.settings.text_color_navigation.alpha != 0.0 and section.settings.text_color_navigation.alpha != blank
    assign text_color_navigation = true
    assign text_color_navigation_section = section.settings.text_color_navigation
  endif    
  assign text_color_hover_navigation = false
  if section.settings.text_color_hover_navigation.alpha != 0.0 and section.settings.text_color_hover_navigation.alpha != blank
    assign text_color_hover_navigation = true
    assign text_color_hover_navigation_section = section.settings.text_color_hover_navigation
  endif    
  assign button_color_navigation = false
  if section.settings.button_color_navigation.alpha != 0.0 and section.settings.button_color_navigation.alpha != blank
    assign button_color_navigation = true
    assign button_color_navigation_section = section.settings.button_color_navigation
  endif
  assign line_color_navigation = false
  if section.settings.line_color_navigation.alpha != 0.0 and section.settings.line_color_navigation.alpha != blank
    assign line_color_navigation = true
    assign line_color_navigation_section = section.settings.line_color_navigation
  endif
  assign slider_color_navigation = false
  if section.settings.slider_color_navigation.alpha != 0.0 and section.settings.slider_color_navigation.alpha != blank
    assign slider_color_navigation = true
    assign slider_color_navigation_section = section.settings.slider_color_navigation
  endif
  assign background_color_navigation = false
  if section.settings.background_color_navigation.alpha != 0.0 and section.settings.background_color_navigation != blank
    assign background_color_navigation = true
    assign background_color_navigation_section = section.settings.background_color_navigation
  endif  
  if section.settings.background_gradient_navigation != blank
    assign background_color_navigation = true
    assign background_color_navigation_section = section.settings.background_gradient_navigation
  endif
-%}  
{% style %}
  {%- if section.settings.enable_sticky -%}
    .site-header {
      --header: sticky;
    }
  {%- endif -%}
  .header .announcement-bar {
    --announcement_icon: {{ section.settings.text_size_customer | plus: 2 }}px;
    {%- if text_color_customer -%}
    --color_text: {{ text_color_customer_section }};
    --color_accent: {{ text_color_customer_section }};
    --color_menu_text: {{ text_color_customer_section }};
    {%- endif -%}
    {%- if text_color_hover_customer -%}
    --color_text_hover: {{ text_color_hover_customer_section }};
    --color_menu_text_hover: {{ text_color_hover_customer_section }};
    {%- endif -%}
    {%- if line_color_customer -%}
    --color_border:{{ line_color_customer_section }};
    {%- endif -%}
    {%- if background_color_customer -%}
    --color_background: {{ background_color_customer_section }};
    {%- endif -%}
    --section_padding: {{ section.settings.padding_customer }}px;
  }
  .header .navigation-bar {
    {%- if text_color_navigation -%}
    --color_title_text:{{ text_color_navigation_section }};
    --color_text: {{ text_color_navigation_section }};
    --color_menu_text: {{ text_color_navigation_section }};
    --color_accent:{{ text_color_navigation_section }};
    --color_accent_modify_25:{{ text_color_navigation_section | color_modify: 'alpha', 0.25 }};
    --button_primary_background:{{ text_color_navigation_section }};
    --button_primary_border:{{ text_color_navigation_section }};
    --button_secondary_border:{{ text_color_navigation_section }};
    --button_secondary_text:{{ text_color_navigation_section }};
    {%- endif -%}
    {%- if text_color_hover_navigation -%}
    --color_text_hover: {{ text_color_hover_navigation_section }};
    --color_menu_text_hover: {{ text_color_hover_navigation_section }};
    {%- endif -%}
    {%- if button_color_navigation -%}  
      --button_primary_text:{{ button_color_navigation_section }};
      --button_secondary_background:{{ button_color_navigation_section }};
    {%- endif -%}
    {%- if slider_color_navigation -%}
    --slider_color:{{ slider_color_navigation_section }};
    --slider_color_modify_25:{{ slider_color_navigation_section | color_modify: 'alpha', 0.25 }};
    {%- endif -%}
    {%- if background_color_navigation -%}
    --color_background_outline:{{ section.settings.background_color_navigation }};
    --color_background: {{ background_color_navigation_section }};
    {%- endif -%}
    --section_padding: {{ section.settings.padding_navigation }}px;
  }    
  {%- if line_color_navigation -%}
    .site-header {
      --color_border_stuck:{{ line_color_navigation_section }};
    }
  {%- endif -%}  
{% endstyle %}
<header data-section-id="{{ section.id }}"  
        data-section-type="header"
        data-mobile="{{ section.settings.menu_location }}"
        data-search="{{ settings.search_predictive_enable }}"
        data-sticky="{% if section.settings.enable_sticky %}true{% else %}false{% endif %}"
        class="header">
  {%- capture site_actions -%}
    <div class="site-actions">
      {% unless request.page_type == 'gift_card' %}
        {% if section.settings.enable_search != 'none' %}
          <div class="site-actions-search{% if section.settings.enable_search_layout != 'cart' %} mobile-hide{% endif %} relative">
            <a href="{{ routes.search_url }}" class="search-form-icon" aria-label="{{ 'search.form.title' | t | escape }}" title="{{ 'search.form.title' | t | escape }}"{% unless request.page_type == 'search' %} header-search-form-link{% endunless %}>{% if section.settings.enable_search == 'text' %}{{ 'search.form.title' | t }}{% else %}{%- render 'icons.theme', id: 'search', size: 20 -%}{% endif %}</a>
            <div class="site-actions-search-form">{%- render 'block.search.results', id: '-desktop', is_header: true -%}</div>
          </div>
        {% endif %}
        <div class="site-actions-cart">
          <a href="{{ routes.cart_url }}" 
             aria-label="{{ 'cart.view_cart' | t | escape }}"
             aria-controls="header-minicart-drawer"
             title="{{ 'cart.view_cart' | t | escape }}" 
             data-site-actions-cart
             {% if section.settings.cart_icon == 'cart-cart' %}data-site-actions-cart-cart{% endif %}>
             {%- liquid
               if section.settings.cart_icon == 'text'
                 echo 'cart.cart' | t
               elsif section.settings.cart_icon == 'cart-cart'
                 echo '<span class="full'
                 if cart.item_count < 1
                   echo ' hidden'
                 endif
                 echo '">'
                 render 'icons.theme', id: 'cart-cart-full'
                 echo '</span>'
                 echo '<span class="empty'
                 if cart.item_count > 0
                   echo ' hidden'
                 endif
                 echo '">'
                 render 'icons.theme', id: 'cart-cart-empty'
                 echo '</span>'
               else
                 render 'icons.theme', id: section.settings.cart_icon
               endif
             -%}
            <span class="site-actions-cart-label {{ section.settings.cart_icon }}" data-cart-item-count>{{ cart.item_count }}</span>
          </a>
        </div>
        {% if shop.customer_accounts_enabled %}
          <div class="site-actions-account{% if section.settings.account_layout != 'cart' %} mobile-hide{% endif %}">
            <a href="{% if customer %}{{ routes.account_url }}{% else %}{{ routes.account_login_url }}{% endif %}" 
               data-text="{% if customer %}{% if customer.name != blank %}{%- render 'account.greeting' -%}{% else %}{{ 'customers.account.header' | t }}{% endif %}{% else %}{{ 'customers.login.header' | t }}{% endif %}" 
               data-url="{% if customer %}{{ routes.account_url }}{% else %}{{ routes.account_login_url }}{% endif %}"
               aria-label="{% if customer %}{{ 'customers.account.header' | t | escape }}{% else %}{{ 'customers.account.login' | t }}{% endif %}" 
               title="{% if customer %}{{ 'customers.account.header' | t | escape }}{% else %}{{ 'customers.account.login' | t }}{% endif %}"
               {% unless request.page_type contains 'customers/' %}data-site-actions-account{% endunless %}>
               {% if section.settings.account == 'icon' %}{%- render 'icons.theme', id: 'user' -%}{% else %}{% if customer %}{% if customer.name != blank %}{%- render 'account.greeting' -%}{% else %}{{ 'customers.account.header' | t }}{% endif %}{% else %}{{ 'customers.account.login' | t }}{% endif %}{% endif %}
            </a>
          </div>
        {% endif %}
      {% else %}
        &nbsp;
      {% endunless %}
    </div>
  {%- endcapture -%}
  {%- assign customer_bar_size = 0 -%}
  {%- capture 'customer_bar' -%}
    {%- liquid
      assign locale_selector = false
      assign currency_selector = false
      if localization.available_languages.size > 1 and section.settings.show_locale_selector
        assign locale_selector = true
      endif
      if localization.available_countries.size > 1 and section.settings.show_currency_selector
          assign currency_selector = true
      endif
    -%}
    <div class="container container-inner">
      {%- liquid 
        assign customer_bar = false
        for block in section.blocks
          case block.type        
            when 'customer_bar'
              capture customer_bar_content
                if block.settings.customer_bar == 'liquid' and block.settings.liquid != blank
                  assign customer_bar_size = customer_bar_size | plus: 1
                  assign customer_bar = true
                  echo block.settings.liquid
                endif
                if block.settings.customer_bar == 'menu' and block.settings.menu != blank
                  assign customer_bar_size = customer_bar_size | plus: 1
                  assign customer_bar = true
                  render 'block.menu', section: section, block: block, ignore: true
                endif
                if block.settings.customer_bar == 'social'
                  assign customer_bar_size = customer_bar_size | plus: 1
                  assign customer_bar = true
                  echo '<div class="social">'
                    render 'block.social', settings: settings
                  echo '</div>'
                endif
                if block.settings.customer_bar == 'text'           
                  assign customer_text_mobile = block.settings.customer_text_mobile
                  assign customer_text_desktop = block.settings.customer_text_desktop     
                  if customer_text_mobile != blank or customer_text_desktop != blank
                    assign customer_bar_size = customer_bar_size | plus: 1
                    assign customer_bar = true
                    if customer_text_mobile != blank  
                      echo '<span class="announcement-bar-text'
                      if customer_text_desktop != blank
                        echo ' mobile'
                      endif
                      echo '">'
                        echo customer_text_mobile
                      echo '</span>'
                    endif             
                    if customer_text_desktop != blank
                      echo '<span class="announcement-bar-text'
                      if customer_text_mobile != blank
                        echo ' desktop'
                      endif
                      echo '">'
                        echo customer_text_desktop
                      echo '</span>'
                    endif
                  endif
                endif
              endcapture
          endcase
        endfor
      -%}      
      <div class="announcements{% unless customer_bar %} right{% endunless %}{% if section.settings.padding_customer_border %} section-border{% endif %}">
        {%- liquid
          echo customer_bar_content           
          echo '<div class="shopify-localization-form-container'               
            if locale_selector or currency_selector
              if section.settings.menu_location == 'opposite'
                echo ' with_site_actions'
              endif
            else
              echo ' MOBILE_HIDDEN'
            endif
            echo '">'
            if locale_selector or currency_selector
              assign customer_bar_size = customer_bar_size | plus: 1
              capture id
                echo 'localization_form_'
                echo section.id
                echo '-LAYOUT'        
              endcapture
              form 'localization', id: id
                render 'localize.language', locale_selector: locale_selector, header: true, layout: 'LAYOUT'
                render 'localize.currency', currency_selector: currency_selector, header: true, layout: 'LAYOUT'
              endform
            endif
            if section.settings.menu_location == 'opposite'
              assign customer_bar_size = customer_bar_size | plus: 1
              echo site_actions
            endif
          echo '</div>'
        -%}
      </div>
    </div>
  {%- endcapture -%}
  {% if customer_bar_size > 0 %}
    <div class="dynamic-section customer-bar announcement-bar body-font-{{ section.settings.text_size_customer }}">
      {{ customer_bar | replace: 'LAYOUT', 'desktop' | replace: ' MOBILE_HIDDEN', '' }}
    </div>
  {% endif %}
  <div class="dynamic-section navigation-bar">
    <div class="container site-header-wrapper site-search-{{ section.settings.enable_search_layout }} site-navigation-{{ section.settings.menu_location }}">
      {% if section.settings.menu != blank %}
        {%- liquid
          if section.settings.enable_search_layout == 'menu'
            assign menu_icon = 'mobilenav-search-toggle'
          else
            assign menu_icon = 'mobilenav-toggle'
          endif
        -%}
        <div class="mobilenav-toggle">      
          <button class="mobilenav-toggle-link" 
                  aria-label="{{ 'header.mobile_menu_open' | t | escape }}"
                  aria-controls="site-mobilenav"
                  data-mobilenav-toggle>{%- render 'icons.theme', id: menu_icon -%}</button>
        </div>
      {% endif %}      
      {% if section.settings.menu_location == 'center' %}
        <div class="site-navigation-spacer">&nbsp;</div>
      {% endif %}      
      <div class="site-logo relative">
        <a href="{{ routes.root_url }}" title="{{ shop.name | escape }}" class="site-logo-header keyed">
          {%- liquid
            if section.settings.logo or section.settings.mobile_logo
              assign desktop_logo = section.settings.logo | default: section.settings.mobile_logo
              if desktop_logo contains '.svg'
                assign desktop_logo_svg = true
                assign desktop_class = 'site-logo-image img_svg'
              else
                assign desktop_logo_svg = false
                assign desktop_class = 'site-logo-image'
              endif
              capture alt
                if section.settings.logo.alt != blank
                  echo section.settings.logo.alt | escape
                else
                  echo shop.name | escape
                endif
              endcapture
              render 'image.load', img_src: desktop_logo, size: section.settings.logo_width, img_svg: desktop_logo_svg, alt: alt, class: desktop_class, logo: true, lazy: 'eager'
              assign mobile_logo = section.settings.mobile_logo | default: section.settings.logo
              if mobile_logo contains '.svg'
                assign mobile_logo_svg = true
                assign mobile_class = 'site-logo-image-mobile img_svg'
              else
                assign mobile_logo_svg = false
                assign mobile_class = 'site-logo-image-mobile'
              endif
              capture alt
                if section.settings.mobile_logo.alt != blank
                  echo section.settings.mobile_logo.alt | escape
                else
                  echo shop.name | escape
                endif
              endcapture
              render 'image.load', img_src: mobile_logo, size: section.settings.mobile_logo_width, img_svg: mobile_logo_svg, alt: alt, class: mobile_class, logo: true, lazy: 'eager'
            else
              echo '<div class="section-title-24 site-logo-text'          
              if section.settings.menu_location == 'center' or section.settings.menu_location == 'mobile'
                echo ' text-center'
              else
                echo ' text-left'
              endif
              echo '" style="margin: 0 auto;max-width: '
              echo section.settings.logo_width
              echo 'px">'
              echo shop.name
              echo '</div>'
            endif
          -%}
        </a>
        {%- render 'image.sizes', mobile_size: section.settings.mobile_logo_width, desktop_size: section.settings.logo_width -%}
      </div>      
      {%- capture nav -%}
        {%- if section.settings.menu_location != 'mobile' -%}
          <nav class="site-navigation" aria-label="{{ 'header.main_menu' | t}}">    
            {%- if section.settings.menu != blank -%}
              {%- render 'header.menu.drop', is_mobile: false, menu_location: section.settings.menu_location, menu: linklists[section.settings.menu], depth: 1, is_submenu: false -%}    
              <div class="site-navigation-overlay"></div>
            {%- endif -%}
          </nav>
        {%- endif -%}
      {%- endcapture -%}      
      {%- liquid
         unless section.settings.menu_location == 'mobile'
          echo nav
        endunless
        echo site_actions
      -%}  
      {% if section.settings.enable_search != 'none' and section.settings.enable_search_layout == 'logo' %}
        <div class="site-search">
          {%- render 'block.search.results', id: '-mobile-header', is_header: true, icon: true, four: true, size: 18 -%}
        </div>
      {% endif %}      
    </div>
  </div>  
</header>
{% if section.settings.menu != blank %}
  <div id="site-mobilenav" class="mobilenav modal__content" data-text="{%- render 'account.greeting' -%}">
    <div class="mobilenav-panel" data-mobilenav-animator>
      {%- liquid
        capture contactbar
          for block in section.blocks
            case block.type
              when 'contact_menu'
              assign link_title = block.settings.heading
              render 'header.menu.contact', is_mobile: true, link_title: link_title, block: block
            endcase
          endfor
        endcapture
      -%}
      <div class="mobilenav-panel-content{% if section.settings.enable_search != 'none' and section.settings.enable_search_layout == 'menu' %} has_search_bar{% endif %}{% if customer_bar_size > 0 %} has_customer_bar{% endif %}{% if contactbar != blank %} has_contactbar{% endif %}">             
        {%- liquid
          if section.settings.enable_search != 'none' and section.settings.enable_search_layout == 'menu'
            render 'block.search.results', id: '-mobile', is_header: true, mobile: true
          endif
          if customer_bar_size > 0
            echo customer_bar | replace: 'LAYOUT', 'mobile' | replace: 'MOBILE_HIDDEN', 'hidden'
          endif
        -%}
        <nav class="mobilenav-navigation scroll-bar-v" aria-label="{{ 'header.mobile_menu' | t}}">
          {%- liquid
            render 'header.menu.drop', is_mobile: true, menu: linklists[section.settings.menu], depth: 1, is_submenu: false
          -%}
        </nav>
        {% if contactbar != blank %}<div class="mobilenav-contactbar contactbar is_mobile" data-contactbar-mobile-target>{{ contactbar }}</div>{% endif %}
      </div>
    </div>
  </div>
{% endif %}
{%- unless request.page_type contains 'customers/' -%}
  <div id="customer" 
       data-section-type="customer" 
       class="site-account account-top {% if customer %}account-columns{% else %}template-account-login-forms{% endif %} modal__content scroll-bar-v" 
       data-text="{%- render 'account.greeting' -%}">
    {% if customer %}  
      <div class="divider">
        <div class="buttons">
          <a href="{{ routes.account_url }}" title="{{ 'customers.account.header' | t | escape }}" class="btn btn-primary">{{ 'customers.account.header' | t }}</a>
          <a href="{{ routes.account_logout_url }}" title="{{ 'customers.account.logout' | t | escape }}" class="btn btn-secondary">{{ 'customers.account.logout' | t }}</a>
        </div>  
        <div class="email"><p>{{ customer.email }} &#8226; <small class="emailupdate"><a href="mailto:{{ shop.email }}?Subject=E-Mail%20Update" title="{{ 'customers.account.update' | t | escape }}" target="_blank">{{ 'customers.account.update' | t }}</a></small></p></div>
      </div>  
      <h5>{% if customer.orders.size > 0 %}{{ 'customers.account.last' | t }}{% else %}{{ 'customers.account.empty' | t }}{% endif %}</h5>
      <div class="divider">
        {% if customer.orders.size > 0 %}
          <ul>
            {% for order in customer.orders limit: 5 %}
              <li>{{ order.name | link_to: order.customer_url }}<span class="date"> - {{ order.created_at | date: format: settings.month_day_year }}</span></li>
            {% endfor %}
          </ul>
          {% if customer.orders.size > 5 %}
            <p><a href="{{ routes.account_url }}" title="{{ 'customers.account.more' | t | escape }}" class="btn btn-primary btn-block arrow">{{ 'customers.account.more' | t | escape }}</a></p>
          {% endif %}
        {% endif %}
      </div>  
      {% if customer.default_address != nil %}
        <div class="address">
          <h5>{{ 'customers.account.default_address' | t }}</h5>
          {%- render 'account.address', address: customer.default_address, account: true %}
        </div>
      {% endif %}  
      {% if customer.addresses_count == 0 %}
        <a href="{{ routes.account_addresses_url }}" title="{{ 'customers.account.new_address_link' | t | escape }}" class="btn btn-primary btn-block arrow bottompad">{{ 'customers.account.new_address_link' | t }}</a>
      {% else %}
        <a href="{{ routes.account_addresses_url }}" title="{{ 'customers.account.view_addresses' | t: count: customer.addresses_count | escape }}" class="btn btn-primary btn-block arrow bottompad">{{ 'customers.account.view_addresses' | t: count: customer.addresses_count }}</a>
      {% endif %}      
    {% else %}
      <div class="account-top template-account-login-forms slider-h-container first-visible" data-login-forms>
        {%- render 'account.login', drawer: true -%}
      </div>
    {% endif %}
  </div>
{%- endunless -%}
{% if request.page_type == 'index' or request.page_type == 'page' %}
  <script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "Organization",
      "name": {{ shop.name | json }},{% if shop.description != blank %}
      "description": {{ shop.description | json }},{% endif %}{% if section.settings.logo != blank %}
      "logo": {{ section.settings.logo | image_url: width:  section.settings.logo_width | prepend:'https:' | json }},{% endif %}
      "url": {% if request.page_type == 'index' %}{{ shop.url | append: routes.root_url | json }}{% else %}{{ shop.url | append: page.url | json }}{% endif %},
      "contactPoint": [{
        "@type": "ContactPoint",
        "telephone": {{ shop.phone | json }},
        "contactType": "Customer service"
    	}],
      "sameAs": [
        {{ settings.social_facebook_link | json }},
        {{ settings.social_threads_link | json }},
        {{ settings.social_x_link | json }},
        {{ settings.social_pinterest_link | json }},
        {{ settings.social_instagram_link | json }},
        {{ settings.social_tiktok_link | json }},
        {{ settings.social_youtube_link | json }},
        {{ settings.social_vimeo_link | json }},
        {{ settings.social_snapchat_link | json }},
        {{ settings.social_tumblr_link | json }},
        {{ settings.social_yelp_link | json }},
        {{ settings.social_foursquare_link | json }},
        {{ settings.social_whatsapp_link | json }},
        {{ settings.social_linkedin_link | json }},
        {{ settings.social_medium_link | json }}
      ]
    }
  </script>
{% endif %}
{% schema %}
  {
    "name": "t:sections.section.titles.header.content",
    "class": "site-main site-header",
    "settings": [
      {
        "type": "checkbox",
        "id": "image_size",
        "label": "t:sections.image.editor.size.label",
        "info": "t:sections.image.editor.size.info",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "enable_sticky",
        "label": "t:sections.header.sticky.label",
        "default": true
      },
      {
        "type": "select",
        "id": "menu_location",
        "label": "t:sections.header.layout.label",
        "default": "right",
        "options": [
          {
            "value": "center",
            "label": "t:sections.header.layout.center.label"
          },
          {
            "value": "right",
            "label": "t:sections.header.layout.right.label"
          },
          {
            "value": "opposite",
            "label": "t:sections.header.layout.opposite.label"
          },
          {
            "value": "below",
            "label": "t:sections.header.layout.below.label"
          },
          {
            "value": "mobile",
            "label": "t:sections.header.layout.mobile.label"
          }
        ]
      },
      {
        "type": "select",
        "id": "enable_search",
        "label": "t:sections.header.search.label",
        "options": [
          {
            "value": "none",
            "label": "t:sections.general.none.label"
          },
          {
            "value": "icon",
            "label": "t:sections.general.icons.icon.label"
          },
          {
            "value": "text",
            "label": "t:sections.general.text.label"
          }
        ],
        "default": "icon"
      },
      {
