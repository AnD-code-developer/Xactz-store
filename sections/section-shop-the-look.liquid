{% if section.blocks.size > 0 %}
  {%- liquid
    assign full_width = section.settings.full_width
    if section.settings.enable_lazy
      assign lazy = 'lazy'
    else
      assign lazy = 'eager'
    endif
    assign text_color = false
    if section.settings.text_color.alpha != 0.0 and section.settings.text_color.alpha != blank
      assign text_color = true
      assign text_color_section = section.settings.text_color
    endif    
    assign text_color_hover = false
    if section.settings.text_color_hover.alpha != 0.0 and section.settings.text_color_hover.alpha != blank
      assign text_color_hover = true
      assign text_color_hover_section = section.settings.text_color_hover
    endif
    assign button_color = false
    if section.settings.button_color.alpha != 0.0 and section.settings.button_color.alpha != blank
      assign button_color = true
      assign button_color_section = section.settings.button_color
    endif
    assign line_color = false
    if section.settings.line_color.alpha != 0.0 and section.settings.line_color.alpha != blank
      assign line_color = true
      assign line_color_section = section.settings.line_color
    endif  
    assign overlay_color = false
    assign overlay_color_opacity = section.settings.overlay_opacity | times: 0.01
    if section.settings.overlay_background_color.alpha != 0.0 and section.settings.overlay_background_color.alpha != blank
      assign overlay_color = true
      assign overlay_color_section = section.settings.overlay_background_color
    endif  
    if section.settings.overlay_background_gradient != blank
      assign overlay_color = true
      assign overlay_color_section = section.settings.overlay_background_gradient
    endif
    assign background_color = false
    if section.settings.background_color.alpha != 0.0 and section.settings.background_color.alpha != blank
      assign background_color = true
      assign background_color_section = section.settings.background_color
    endif
    if section.settings.background_gradient != blank
      assign background_color = true
      assign background_color_section = section.settings.background_gradient
    endif
  -%}  
  {% style %}
    .dynamic-section-{{ section.id }} {
      --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
      --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
      --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
      --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
      --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
      --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px;
      {%- if text_color -%}
        --color_title_text:{{ text_color_section }};
        --color_text:{{ text_color_section }};
        --color_accent:{{ text_color_section }};
        --color_text_modify_5:{{ text_color_section }};
        --color_text_modify_25:{{ text_color_section | color_modify: 'alpha', 0.25 }};
        --color_accent:{{ text_color_section }};
        --color_accent_modify_25:{{ text_color_section | color_modify: 'alpha', 0.25 }};
        --color_error:{{ text_color_section }};
        --color_rating:{{ text_color_section }};
        --color_sale:{{ text_color_section }};
        --button_primary_background:{{ text_color_section }};
        --button_primary_border:{{ text_color_section }};
        --button_secondary_border:{{ text_color_section }};
        --button_secondary_text:{{ text_color_section }};
      {%- endif -%}
      {%- if text_color_hover -%}
        --color_text_hover:{{ text_color_hover_section }};
      {%- endif -%}
      {%- if button_color -%}  
        --button_primary_text:{{ button_color_section }};
        --button_secondary_background:{{ button_color_section }};
      {%- endif -%}
      {%- if line_color -%}
        --color_border:{{ line_color_section }};
      {%- endif -%}
      {%- if overlay_color -%}
        --color_overlay:{{ overlay_color_section }};
        --color_overlay_opacity:{{ overlay_color_opacity }};
      {%- endif -%}
      {%- if background_color -%}
        --color_background_outline:{{ section.settings.background_color }};
        --color_background:{{ background_color_section }};
        --color_background_section:{{ background_color_section }};
      {%- endif -%}
    }
  {% endstyle %}  
  <div class="dynamic-section dynamic-section-{{ section.id }}{% if full_width %} section-background{% endif %}">
    <section data-section-id="{{ section.id }}"
             data-section-type="shop-look-{{ section.id }}"
             data-url="{{ 'script.look.js' | asset_url }}"
             {% if full_width %}class="section-skip"{% endif %}>
      <div class="section-padding-top section-padding-bottom section-margin-bottom{% if section.settings.padding_border %} section-border{% endif %}{% unless full_width %} section-background{% endunless %}">
        <div class="section-layout{% if full_width %} full_width{% endif %}">          
          <div class="shop-look">          
            <div class="shop-look-content scroll-bar-h" aria-hidden="true" hidden>          
              <button type="button" class="close-button close-button-shop-look alt" aria-label="{{ 'general.form.close' | t | escape }}" data-look-close>{%- render 'icons.theme', id: 'close', size: 18 -%}</button>
              {% for block in section.blocks %}
                {%- case block.type -%}
                  {%- when 'product' -%}                  
                    {%- liquid
                      echo '<div id="hotspot-'
                      echo block.id
                      echo '-'
                      echo forloop.index
                      echo '" class="container container-inner section-card-wrapper skip" data-shop-look aria-hidden="true" hidden>'
                        echo '<div class="section-card-rows product-rows">'                       
                          if block.settings.product_handle != blank
                            assign product = all_products[block.settings.product_handle]
                            assign size = 440
                            assign sizes = '(min-width: 500px) 440px, calc(100vw - 40px)'
                            render 'block.product.loop', section: section, product: product, forloop: forloop, size: size, sizes: sizes, lazy: lazy, shop_look: true
                          else
                            render 'block.onboard', section: section, size: 440, product: true, forloop: forloop
                          endif
                        echo '</div>'
                      echo '</div>'
                    -%}
                  {%- when 'custom' -%}  
                    {%- liquid                       
                      assign size = 440
                      assign sizes = '(min-width: 500px) 440px, calc(100vw - 40px)'
                      render 'block.image_text', section: section, block: block, padding: block.settings.image_height, mobile_size: size, mobile_sizes_final: sizes, desktop_size: size, desktop_sizes_final: sizes, forloop: forloop, lazy: lazy, shop_look: true, skip: true
                    -%}
                {%- endcase -%}
              {% endfor %}
            </div>  
            <div class="shop-look-hotspots hotspots body-slide-container">      
              {%- liquid
                if section.settings.image_desktop != blank or section.settings.image_mobile != blank
                  if full_width
                    assign desktop_size = section.settings.image_desktop.width
                    assign desktop_sizes = '100dvw'
                  else
                    assign desktop_size = 1240
                    assign desktop_sizes = '(min-width: 1280px) 1240px, (min-width: 960px) calc(100dvw - 40px), 100dvw'
                  endif
                  assign mobile_size = 768
                  capture mobile_sizes
                    echo '(min-width: 1280px) '
                    if section.settings.image_desktop == blank
                      echo desktop_size
                    else
                      echo mobile_size
                    endif
                    echo 'px, (min-width: 960px) calc(100dvw - 40px), 100dvw'
                  endcapture
                  if section.settings.enable_lazy
                    assign lazy = 'lazy'
                  else
                    assign lazy = 'eager'
                  endif
                endif
                render 'block.images', section: section, block: section, mobile_size: mobile_size, mobile_sizes: mobile_sizes, desktop_size: desktop_size, desktop_sizes: desktop_sizes, image_background: false, full_width: full_width, lazy: lazy, custom: true
              -%}      
              {% if section.settings.image_desktop == blank and section.settings.image_mobile == blank %}  
                <div class="section-layout-wrapper">
                  <div class="mixed">                    
                    {%- liquid
                      echo 'hero-apparel-3' | placeholder_svg_tag: 'placeholder-svg'
                      if section.settings.overlay_opacity != 0 and overlay_color
                        echo '<div class="section-layout-overlay skip"></div>'
                      endif
                    -%}
                  </div>
                </div>
              {% endif %}      
              {%- liquid
                unless section.settings.image_desktop == blank and section.settings.image_mobile == blank
                  if section.settings.overlay_opacity != 0 and overlay_color
                    echo '<div class="section-layout-overlay skip"></div>'
                  endif
                endunless
                if full_width
                  assign size = 1900
                else
                  assign size = 1240
                endif
                render 'image.sizes', note: true, mobile_size: 768, desktop_size: size
              -%}    
              {% if section.blocks.size > 0 %}
                {% for block in section.blocks %}    
                  {% style %}
                    .dynamic-block-{{ block.id }} {
                      {%- if block.settings.hotspot_color.alpha != 0.0 and block.settings.hotspot_color.alpha != blank -%}
                        --hotspot_color_rgba: {{ block.settings.hotspot_color | color_modify: 'alpha', 1 }};
                        --hotspot_color_rgba_0: {{ block.settings.hotspot_color | color_modify: 'alpha', 0 }};
                        --hotspot_color_rgba_70: {{ block.settings.hotspot_color | color_modify: 'alpha', 0.7 }};
                      {%- endif -%}
                      --hotspot_size: {{ block.settings.hotspot_size }}px;
                      --position_left_mobile: {{ block.settings.hotspot_x_mobile }}%;
                      --position_left_desktop: {{ block.settings.hotspot_x_desktop }}%;
                      --position_top_mobile: {{ block.settings.hotspot_y_mobile }}%;
                      --position_top_desktop: {{ block.settings.hotspot_y_desktop }}%;
                      --position_delay: {{ forloop.index | times: 2 | prepend: '0.' | append: 's' }};
                    }
                  {% endstyle %}
                  {%- assign size = block.settings.hotspot_size | times: 0.33 -%}
                  {%- case block.type -%}
                    {%- when 'product' -%}
                      <button class="dynamic-block-{{ block.id }} hotspot active{% if settings.animation_enable %} animated-item animated-delay-{{ forloop.index0 }}{% endif %}"{% if block.settings.product_handle != blank %} title="{{ 'general.general.click_html' | t: link: block.settings.product_handle.title }}" aria-label="{{ 'general.general.click_html' | t: link: block.settings.product_handle.title }}"{% else %} title="{{ 'general.general.click_open' }}" tabindex="0"{% endif %} data-id="hotspot-{{ block.id }}-{{ forloop.index }}" {{ block.shopify_attributes }}><span>{% if block.settings.product_handle != blank %}{%- render 'icons.theme', id: 'arrow-l', size: size -%}{% endif %}</span></button>
                    {%- when 'custom' -%}
                      <button class="dynamic-block-{{ block.id }} hotspot active{% if settings.animation_enable %} animated-item animated-delay-{{ forloop.index0 }}{% endif %}"{% if block.settings.hotspot_seo != blank %} title="{{ block.settings.hotspot_seo }}" aria-label="{{ block.settings.hotspot_seo }}"{% else %} title="{{ 'general.general.click_open' }}" tabindex="0"{% endif %} data-id="hotspot-{{ block.id }}-{{ forloop.index }}" {{ block.shopify_attributes }}><span>{%- render 'icons.theme', id: 'arrow-l', size: size -%}</span></button>
                  {%- endcase -%}
                {% endfor %}
              {% endif %}              
              <div class="body-slide-background look-slide-background" data-body-slide-background></div>
            </div>   
          </div>  
        </div>
      </div>
    </section>
  </div>
{% endif %}
