{%- liquid
  assign items_count = section.settings.items_count
  paginate search.results by items_count
  	assign total_products = paginate.items
  endpaginate
  if section.settings.enable_lazy
    assign lazy = 'lazy'
  else
    assign lazy = 'eager'
  endif
  assign items_per_row_mobile = section.settings.items_per_row_mobile
  assign items_per_row_desktop = section.settings.items_per_row_desktop  
  if section.settings.include_border
    assign border_width = 2
    assign padding_width = 40
  else
    assign border_width = 0
    assign padding_width = 0
  endif  
  assign total = 20 | plus: border_width | plus: padding_width | append: 'px'
  case items_per_row_mobile
    when 1
      assign mobile_size = 710
    when 2
      assign mobile_size = 355
    when 3
      assign mobile_size = 236.66
  endcase  
  capture mobile_sizes_product
    echo 'calc((100vw / '
    echo items_per_row_mobile
    echo ') - ('
    echo section.settings.padding_grid_mobile
    echo 'px * ('
    echo items_per_row_mobile
    echo ' - 1) / '
    echo items_per_row_mobile
    echo ') - 20px)'
  endcapture  
  capture mobile_sizes_blog
    echo 'calc((100vw / '
    echo items_per_row_mobile
    echo ') - ('
    echo section.settings.padding_grid_mobile
    echo 'px * ('
    echo items_per_row_mobile
    echo ' - 1) / '
    echo items_per_row_mobile
    echo ') - '
    echo total
    echo ')'
  endcapture
  case items_per_row_desktop
    when 2
      assign desktop_size = 620
    when 3
      assign desktop_size = 413
    when 4
      assign desktop_size = 310
    when 5
      assign desktop_size = 248
  endcase  
  capture desktop_sizes_product
    echo 'calc((100vw / '
    echo items_per_row_desktop
    echo ') - ('
    echo section.settings.padding_grid_desktop
    echo 'px * ('
    echo items_per_row_desktop
    echo ' - 1) / '
    echo items_per_row_desktop
    echo ') - 20px)'
  endcapture  
  capture desktop_sizes_blog
    echo 'calc((100vw / '
    echo items_per_row_desktop
    echo ') - ('
    echo section.settings.padding_grid_desktop
    echo 'px * ('
    echo items_per_row_desktop
    echo ' - 1) / '
    echo items_per_row_desktop
    echo ') - '
    echo total
    echo ')'
  endcapture  
  assign mobile_column_padding = items_per_row_mobile | minus: 1 | times: section.settings.padding_grid_mobile | divided_by: items_per_row_mobile
  assign desktop_column_padding = items_per_row_desktop | minus: 1 | times: section.settings.padding_grid_desktop | divided_by: items_per_row_desktop  
  if mobile_size > desktop_size
    assign size_product = mobile_size | minus: mobile_column_padding
    assign size_blog = mobile_size | minus: border_width | minus: padding_width | minus: mobile_column_padding
  else
    assign size_product = desktop_size | minus: desktop_column_padding
    assign size_blog = desktop_size | minus: border_width | minus: padding_width | minus: desktop_column_padding
  endif  
  capture sizes_product
    echo '(min-width: 1280px) '
    echo desktop_size | minus: desktop_column_padding
    echo 'px, (min-width: 768px) '
    echo desktop_sizes_product
    echo ', '
    echo mobile_sizes_product
  endcapture    
  capture sizes_blog
    echo '(min-width: 1280px) '
    echo desktop_size | minus: border_width | minus: padding_width | minus: desktop_column_padding
    echo 'px, (min-width: 768px) '
    echo desktop_sizes_blog
    echo ', '
    echo mobile_sizes_blog
  endcapture  
  assign lazy_total = items_per_row_mobile | times: 2
  assign text_color = false
  if section.settings.text_color.alpha != 0.0 and section.settings.text_color.alpha != blank
    assign text_color = true
    assign text_color_section = section.settings.text_color
  endif
  assign text_color_hover = false
  if section.settings.text_color_hover.alpha != 0.0 and section.settings.text_color_hover.alpha != blank
    assign text_color_hover = true
    assign text_color_hover_section = section.settings.text_color_hover
  endif
  assign button_color = false
  if section.settings.button_color.alpha != 0.0 and section.settings.button_color.alpha != blank
    assign button_color = true
    assign button_color_section = section.settings.button_color
  endif
  assign line_color = false
  if section.settings.line_color.alpha != 0.0 and section.settings.line_color.alpha != blank
    assign line_color = true
    assign line_color_section = section.settings.line_color
  endif
  assign background_color = false
  if section.settings.background_color.alpha != 0.0 and section.settings.background_color.alpha != blank
    assign background_color = true
    assign background_color_section = section.settings.background_color
  endif
  if section.settings.background_gradient != blank
    assign background_color = true
    assign background_color_section = section.settings.background_gradient
  endif
-%}
{% style %}
  .dynamic-section-{{ section.id }} {
    --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
    --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
    --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
    --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
    --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
    --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px; 
    --grid-mobile: {{ section.settings.padding_grid_mobile }}px;
    --grid-desktop: {{ section.settings.padding_grid_desktop }}px;
    --content_width_mobile: 100%;
    --content_width_desktop: 100%;  
    {%- if text_color -%}
    --color_title_text:{{ text_color_section }};
    --color_text:{{ text_color_section }};
    --color_text_modify_5:{{ text_color_section | color_modify: 'alpha', 0.5 }};
    --color_accent:{{ text_color_section }};
    --button_primary_background:{{ text_color_section }};
    --button_primary_border:{{ text_color_section }};
    --button_secondary_border:{{ text_color_section }};
    --button_secondary_text:{{ text_color_section }};
    {%- endif -%}
    {%- if text_color_hover -%}
    --color_text_hover:{{ text_color_hover_section }};
    {%- endif -%}
    {%- if line_color -%}
    --color_border:{{ line_color_section }};
    {%- endif -%}
    {%- if button_color -%}  
      --button_primary_text:{{ button_color_section }};
      --button_secondary_background:{{ button_color_section }};
      --color_accent_hover:{{ button_color_section }};
    {%- endif -%}
    {%- if background_color -%}
      --color_background_outline:{{ section.settings.background_color }};
      --color_background:{{ background_color_section }};
      --color_background_section:{{ background_color_section }};
    {%- endif -%}
  }
{% endstyle %}
<div class="dynamic-section dynamic-section-{{ section.id }} section-background">
  <section data-section-id="{{ section.id }}" 
           data-section-type="search"
           data-slider-breakpoint="none"    
           data-sort-by="{{ search.sort_by | default: search.default_sort_by }}"
           data-pagination-method="traditional_pagination"
           data-products-per-page="{{ items_count }}"
           data-total-products="{{ total_products | json }}"
           data-text-cancel="{{ 'general.form.cancel' | t | escape }}"
           id="collection"
           class="relative overflow_hidden">
    <div class="section-padding-top section-padding-bottom section-margin-bottom{% if section.settings.padding_border %} section-border{% endif %}">
      <div class="collection-container section-card-wrapper section-card-wrapper-left">
        {% paginate search.results by items_count %}
          {%- liquid
            assign product_first = false
            assign product_count = 0
            assign article_first = false
            assign article_count = 0
            assign page_first = false
            assign page_count = 0    
            for item in search.results
              if item.object_type == 'product'
                if forloop.first
                  assign product_first = true
                endif
                assign product_count = product_count | plus: 1
              endif
              if item.object_type == 'article'
                if forloop.first
                  assign article_first = true
                endif
                assign article_count = article_count | plus: 1
              endif
              if item.object_type == 'page'
                if forloop.first
                  assign page_first = true
                endif
                assign page_count = page_count | plus: 1
              endif
            endfor    
            assign type_count = 0
            if product_count > 0
              assign type_count = type_count | plus: 1
            endif
            if article_count > 0
              assign type_count = type_count | plus: 1
            endif
            if page_count > 0
              assign type_count = type_count | plus: 1
            endif
            if section.settings.enable_filters and product_count > 0
              render 'collection.filters', section: section, collection: search
            endif
          -%}          
          <div id="collection-slider" class="body-slide-container">                  
            {%- render 'block.search', section: section, is_page: true, type_count: type_count, product_count: product_count, product_first: product_first,page_count: page_count, page_first: page_first, article_count: article_count, article_first: article_first -%}
              {% if product_count > 0 %}
                {% if section.settings.enable_filters or section.settings.enable_sorting or section.settings.enable_breadcrumbs %}
                  <div class="collection-filter-sortby">          
                    {% if section.settings.enable_filters %}
                      <div class="collection-filter">
                        <button type="button" class="keyed-inside" aria-label="{{ 'collection.refine.button_title' | t }}" data-filter>{%- render 'icons.theme', id: 'filter' -%}</button>          
                      </div>
                      {% if section.settings.enable_sorting or section.settings.enable_breadcrumbs %}<span class="separator"></span>{% endif %}
                    {% endif %}          
                    <div class="sort-bread">          
                      {%- liquid
                        if section.settings.enable_breadcrumbs
                          render 'breadcrumbs'
                        endif
                      -%}          
                      {% if section.settings.enable_sorting %}
                        {% if section.settings.enable_breadcrumbs %}<span class="separator"></span>{% endif %}
                        <div class="option-select" data-select-wrapper data-select-wrapper-sortby>
                          <label class="visually-hidden" for="collection-sortby" data-select-label>
                            {%- liquid
                              assign current_sort = search.sort_by | default: search.default_sort_by
                              for option in search.sort_options
                                if option.value == current_sort
                                  echo option.name
                                endif
                              endfor
                            -%}
                          </label>
                          <select class="option-select-input scroll-bar-v" id="collection-sortby" data-select>
                            {% for option in search.sort_options %}
                              <option value="{{ option.value }}"{% if current_sort contains option.value %} selected{% endif %}>{{ option.name }}</option>
                            {% endfor %}
                          </select>
                          <span class="option-select-arrow">{%- render 'icons.theme', id: 'arrow', size: 10 -%}</span>
                        </div>                  
                      {% endif %}          
                    </div>          
                  </div>
                {% endif %}
              {% endif %}
              <div id="collection-grid" class="container-inner">
                {%- liquid
                  if product_count > 0
                    echo '<div id="search-products" class="section-card-rows mobile-rows-of-'
                    echo items_per_row_mobile
                    echo ' desktop-rows-of-'
                    echo items_per_row_desktop
                    echo ' product-rows search-results-group'
                    if product_first
                      echo ' selected visible'                
                    endif
                    echo ' search-results-products slider-v slider-h'                    
                    if page_count > 0 or article_count > 0
                      echo ' slider-h-1'
                    endif                
                    echo '"'                
                    echo '>'
                      for item in search.results
                        if item.object_type != 'product'
                          continue
                        endif
                        render 'block.product.loop', section: section, product: item, is_collection: true, forloop: forloop, size: size_product, sizes: sizes_product, lazy: lazy, lazy_total: lazy_total
                      endfor
                    echo '<div class="body-slide-background quick-slide-background" data-body-slide-background></div></div>'
                  endif
                  if page_count > 0
                    echo '<div id="search-pages" class="section-card-rows mobile-rows-of-'
                    echo items_per_row_mobile
                    echo '  desktop-rows-of-'
                    echo items_per_row_desktop
                    echo ' product-rows search-results-group'
                    if page_first
                      echo ' selected visible'
                    endif
                    echo ' search-results-pages slider-v slider-h'                    
                    if product_count > 0
                      echo ' slider-h-2'
                    else
                      echo ' slider-h-1'
                    endif                
                    echo '"'                      
                    echo '>'
                      for item in search.results
                        if item.object_type != 'page'
                          continue
                        endif
                        render 'block.article.loop', section: section, article: item, is_page: true, forloop: forloop, excerpt: section.settings.excerpt_limit
                      endfor
                    echo '</div>'
                  endif
                  if article_count > 0
                    echo '<div id="search-articles" class="section-card-rows mobile-rows-of-'
                    echo items_per_row_mobile
                    echo '  desktop-rows-of-'
                    echo items_per_row_desktop
                    echo ' product-rows search-results-group'
                    if article_first
                      echo ' selected visible'
                    endif
                    echo ' search-results-articles slider-v slider-h'                    
                    if product_count > 0 and page_count > 0
                      echo ' slider-h-3'
                    elsif product_count > 0 or page_count > 0
                      echo ' slider-h-2'
                    else
                      echo ' slider-h-1'
                    endif
                    echo '"'
                    echo '>'
                      for item in search.results
                        if item.object_type != 'article'
                          continue
                        endif
                        render 'block.article.loop', section: section, article: item, size: size_blog, sizes: sizes_blog, forloop: forloop, lazy: lazy, lazy_total: lazy_total, padding: section.settings.image_height, excerpt: section.settings.excerpt_limit
                      endfor
                    echo '</div>'
                  endif
                -%}
              {%- liquid
                for block in section.blocks
                  render block
                endfor
              -%}    
              </div>  
              <div id="pagination" data-total-products="{{ total_products | json }}">
                {%- render 'pagination', paginate: paginate, title: page_title -%}
              </div>
            <div class="body-slide-background quick-slide-background collection-page" data-body-slide-background></div>
            </div>  
        {% endpaginate %}        
      </div>
    </div>
  </section>
</div>
