{% if section.blocks.size > 0 %} 
  {%- liquid
    assign full_width = section.settings.full_width  
    assign blocks_size = 0
    assign blocks_index = 0 
    assign background_color = false
    if section.settings.background_color.alpha != 0.0 and section.settings.background_color.alpha != blank
      assign background_color = true
      assign background_color_section = section.settings.background_color
    endif
    if section.settings.background_gradient != blank
      assign background_color = true
      assign background_color_section = section.settings.background_gradient
    endif
  -%}  
  {% style %}
    .dynamic-section-{{ section.id }} { 
      --section_height_mobile: {{ section.settings.section_height_mobile }}vh; 
      --section_height_desktop: {{ section.settings.section_height_desktop }}vh;
      --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
      --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
      --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
      --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
      --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
      --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px; 
      {%- if background_color -%}
        --color_background_outline:{{ section.settings.background_color }};
        --color_background:{{ background_color_section }};
        --color_background_section:{{ background_color_section }};
      {%- endif -%}
    }    
  {% endstyle %}  
  {%- liquid
    for block in section.blocks
      case block.type
        when 'subject'
          if block.settings.layout_mobile == 'below'
            assign layout_mobile = true
          else
            assign layout_mobile = false
          endif
          capture block_subject
            render 'block.subject', section: section, block: block: is_background: true, layout_mobile: layout_mobile
          endcapture
        when 'image'
          assign blocks_size = blocks_size | plus: 1
      endcase
    endfor
  -%}  
  <div class="dynamic-section dynamic-section-{{ section.id }}{% if full_width %} section-background{% endif %}">
    <section data-section-id="{{ section.id }}" 
             {% if blocks_size > 1 %}
               data-section-type="before-after-{{ section.id }}"
               data-url="{{ 'script.before-after.js' | asset_url }}" 
             {% endif %}
             class="before-after hotspots relative{% if full_width %} section-skip{% endif %}">
      <div class="{% if layout_mobile %}section-layout-stack {% endif %}section-padding-top section-padding-bottom section-margin-bottom{% if section.settings.padding_border %} section-border{% endif %}{% unless full_width %} section-background{% endunless %}">        
        {{ block_subject }}
        <div class="relative section-layout{% if full_width %} full_width skip{% endif %} height-padding-custom desktop">
          {% for block in section.blocks reversed %}      
            {% case block.type %}
              {% when 'image' %}              
                {%- liquid      
                  assign blocks_index = blocks_index | plus: 1
                  assign last_block = false
                  case blocks_size
                    when 1
                      assign last_block = true
                      assign bORa = ''
                      assign img_pos = 'background-image'
                    when 2
                      case blocks_index
                        when 2
                          assign bORa = 'after'
                          assign img_pos = 'foreground'
                        when 1
                          assign last_block = true
                          assign bORa = ''
                          assign img_pos = 'background-image'
                      endcase
                    when 3                       
                    case blocks_index
                      when 3
                        assign bORa = 'after'
                        assign img_pos = 'foreground'
                      when 2
                        assign bORa = 'before'
                        assign img_pos = 'background'
                      when 1
                        assign last_block = true
                        assign bORa = ''
                        assign img_pos = 'background-image'
                    endcase
                  endcase
                  unless last_block
                    assign hotspot_background_color = false
                    if block.settings.hotspot_background_color.alpha != 0.0 and block.settings.hotspot_background_color.alpha != blank
                      assign hotspot_background_color = true
                      assign hotspot_background_color_block = block.settings.hotspot_background_color
                    endif
                    assign hotspot_line_color = false
                    if block.settings.hotspot_line_color.alpha != 0.0 and block.settings.hotspot_line_color.alpha != blank
                      assign hotspot_line_color = true
                      assign hotspot_line_color_block = block.settings.hotspot_line_color
                    endif  
                  endunless
                  assign overlay_color = false
                  assign overlay_color_opacity = block.settings.overlay_opacity | times: 0.01
                  if block.settings.overlay_background_color.alpha != 0.0 and block.settings.overlay_background_color.alpha != blank
                    assign overlay_color = true
                    assign overlay_color_block = block.settings.overlay_background_color
                  endif                       
                  if block.settings.overlay_background_gradient != blank
                    assign overlay_color = true
                    assign overlay_color_block = block.settings.overlay_background_gradient
                  endif
                  assign hotspot_x_mobile = block.settings.hotspot_x_mobile
                  assign hotspot_x_desktop = block.settings.hotspot_x_desktop
                  assign hotspot_y_mobile = block.settings.hotspot_y_mobile
                  assign hotspot_y_desktop = block.settings.hotspot_y_desktop
                -%}          
                <div class="dynamic-block-{{ block.id }} section-layout{% if full_width %} full_width skip{% endif %} relative before-after-img" {{ block.shopify_attributes }}>
                  <div id="{{ img_pos }}-img-{{ section.id }}" class="before-after-img {{ img_pos }}-img {{ block.settings.direction }}">
                    {% style %}
                      .dynamic-block-{{ block.id }} {                          
                        {%- if block.settings.image_desktop != blank -%}--image_height_desktop: {{ block.settings.image_desktop.height }}px;{%- endif -%}
                        {%- if block.settings.image_mobile != blank -%}--image_height_mobile: {{ block.settings.image_mobile.height }}px;{%- endif -%}
                        {%- unless last_block -%}
                          --hotspot_size: {{ block.settings.hotspot_size }}px;
                          --position_left_mobile: {{ hotspot_x_mobile }}%;
                          --position_left_desktop: {{ hotspot_x_desktop }}%;
                          --position_top_mobile: {{ hotspot_y_mobile }}%;
                          --position_top_desktop: {{ hotspot_y_desktop }}%;                          
                          {%- if hotspot_background_color -%}
                            --hotspot_color_rgba: {{ hotspot_background_color_block | color_modify: 'alpha', 1 }};
                            --hotspot_color_rgba_0: {{ hotspot_background_color_block | color_modify: 'alpha', 0 }};
                            --hotspot_color_rgba_70: {{ hotspot_background_color_block | color_modify: 'alpha', 0.7 }};
                          {%- endif -%}                          
                          {%- if hotspot_line_color -%}
                            --color_text: {{ hotspot_line_color_block }};
                          {%- endif -%}
                        {%- endunless -%}
                        {%- if overlay_color -%}
                          --color_overlay:{{ overlay_color_block }};
                          --color_overlay_opacity:{{ overlay_color_opacity }};
                        {%- endif -%}
                        --position_delay: {{ blocks_index | times: 2 | prepend: '0.' | append: 's' }};
                      }
                    {% endstyle %}
                    {%- liquid
                      if block.settings.image_desktop != blank or block.settings.image_mobile != blank
                       
                        if full_width
                          assign desktop_size = block.settings.image_desktop.width
                          assign desktop_sizes = '100dvw'
                        else
                          assign desktop_size = 1240
                          assign desktop_sizes = '(min-width: 1280px) 1240px, (min-width: 960px) calc(100dvw - 40px), 100dvw'
                        endif
                        assign mobile_size = 768
                        capture mobile_sizes
                          echo '(min-width: 1280px) '
                          if block.settings.image_desktop == blank
                            echo desktop_size
                          else
                            echo mobile_size
                          endif
                          echo 'px, (min-width: 960px) calc(100dvw - 40px), 100dvw'
                        endcapture
                        if section.settings.enable_lazy
                          assign lazy = 'lazy'
                        else
                          assign lazy = 'eager'
                        endif
                      endif
                      render 'block.images', section: section, block: block, mobile_size: mobile_size, mobile_sizes: mobile_sizes, desktop_size: desktop_size, desktop_sizes: desktop_sizes, image_background: true, full_width: full_width, lazy: lazy, forloop: forloop, has_onboard: true                      
                      unless block.settings.image_desktop == blank and block.settings.image_mobile == blank
                        if block.settings.overlay_opacity != 0 and overlay_color
                          echo '<div class="section-layout-overlay"></div>'
                        endif
                      endunless
                      if full_width
                        assign size = 1900
                      else
                        assign size = 1240
                      endif
                      render 'image.sizes', note: true, mobile_size: 768, desktop_size: size, padding_mobile: section.settings.section_height_mobile, padding_desktop: section.settings.section_height_desktop, new_size_mobile: 768, new_size_desktop: 1240
                    -%}
                    {% if block.settings.link != blank %}
                      <a href="{{ block.settings.link }}" class="section-layout-link keyed-inside"{% if block.settings.link_seo != blank %} title="{{ block.settings.link_seo | escape }}"{% endif %}></a>
                    {% endif %}
                  </div>
                  {% unless last_block %}                        
                    {% if block.settings.hotspot_seo != blank %} 
                      <label for="{{ bORa }}-{{ section.id }}" class="visually-hidden">{{ block.settings.hotspot_seo }}</label>
                    {% endif %}
                    <input type="range"
                           min="0"
                           max="100"
                           value="{{ hotspot_x_desktop }}"
                           id="{{ bORa }}-{{ section.id }}"
                           class="before-after-slider"
                          {% if block.settings.hotspot_seo != blank %} 
                            title="{{ block.settings.hotspot_seo }}" 
                          {% endif %}
                           data-mobile="{{ hotspot_x_mobile }}"
                           data-desktop="{{ hotspot_x_desktop }}"
                           data-direction="{{ block.settings.direction }}"
                           data-img="{{ img_pos }}-img-{{ section.id }}"
                           data-hotspot="{{ img_pos }}-hotspot-{{ section.id }}">
                    {%- assign size = block.settings.hotspot_size | divided_by: 2 -%}
                    <button id="{{ img_pos }}-hotspot-{{ section.id }}"
                            class="before-after-button hotspot active"
                            aria-hidden="true"
                            tabindex="-1"
                            data-id="hotspot-{{ block.id }}-{{ forloop.index }}" {{ block.shopify_attributes }}><span>{%- render 'icons.theme', id: 'arrow-double', size: size -%}</span></button>
                  {% endunless %}                  
                </div>
            {% endcase %}
          {% endfor %}
          {% if blocks_size == 0 %}  
            <div class="section-layout-wrapper">
              <div class="height-padding-custom mixed"></div>
            </div>
          {% endif %}
        </div>
      </div>
    </section>
  </div>
{% endif %}
