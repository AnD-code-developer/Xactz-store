{% if section.blocks.size > 0 %}
  {%- liquid
    assign full_width = section.settings.full_width
    assign blocks_size = 0
    assign block_width_left = 1
    assign block_width_right = 0
    assign block_width = 0
    for block in section.blocks
      case block.type
        when 'subject' 
          assign block_layout_x = block.settings.layout_x
          assign block_layout_y = block.settings.layout_y
          assign block_width_max = section.settings.items_per_row_desktop | minus: 1    
          assign block_width = block.settings.content_width | at_most: block_width_max
          if block_layout_x == 'right'
            assign block_width_right = 100 | divided_by: section.settings.items_per_row_desktop | times: block_width | at_most: 100 | at_least: 0
            assign block_width_left = 100 | minus: block_width_right | at_most: 100 | at_least: 0
          else
            assign block_width_left = 100 | divided_by: section.settings.items_per_row_desktop | times: block_width | at_most: 100 | at_least: 0
            assign block_width_right = 100 | minus: block_width_left | at_most: 100 | at_least: 0
          endif
          assign content_width_desktop = 100 | divided_by: section.settings.items_per_row_desktop | times: block_width
          capture block_subject
            render 'block.subject', section: section, block: block, content_width_mobile: 100, content_width_desktop: content_width_desktop, block_subject: true
          endcapture
        when 'icon'
          assign blocks_size = blocks_size | plus: 1
      endcase
    endfor
    assign enable_slider = false  
    assign enable_slider_mobile = section.settings.enable_slider_mobile
    assign enable_slider_desktop = section.settings.enable_slider_desktop
    if enable_slider_mobile or enable_slider_desktop  
      assign enable_slider = true  
      if enable_slider_mobile and enable_slider_desktop == false
        assign enable_slider_size = 'small'
      elsif enable_slider_mobile == false and enable_slider_desktop
        assign enable_slider_size = 'large-only'
      else
        assign enable_slider_size = 'large'
      endif  
    endif  
    assign slider_color = false
    if section.settings.slider_color.alpha != 0.0 and section.settings.slider_color.alpha != blank
      assign slider_color = true
      assign slider_color_section = section.settings.slider_color
    endif
    assign background_color = false
    if section.settings.background_color.alpha != 0.0 and section.settings.background_color.alpha != blank
      assign background_color = true
      assign background_color_section = section.settings.background_color
    endif
    if section.settings.background_gradient != blank
      assign background_color = true
      assign background_color_section = section.settings.background_gradient
    endif
  -%}  
  {% style %}
    .dynamic-section-{{ section.id }} {
      --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
      --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
      --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
      --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
      --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
      --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px;    
      --content_width_mobile: 100%;
      --content_width_desktop: 100%;    
      --block_width_left: {{ block_width_left }}%;
      --block_width_right: {{ block_width_right }}%;
      --grid-mobile: {{ section.settings.padding_grid_mobile }}px;
      --grid-desktop: {{ section.settings.padding_grid_desktop }}px;
      {%- if slider_color -%}
      --slider_color:{{ slider_color_section }};
      --slider_color_modify_25:{{ slider_color_section | color_modify: 'alpha', 0.25 }};
      {%- endif -%}
      {%- if background_color -%}
        --color_background_outline:{{ section.settings.background_color }};
        --color_background:{{ background_color_section }};
        --color_background_section:{{ background_color_section }};
      {%- endif -%}
    }    
  {% endstyle %}  
  <div class="dynamic-section dynamic-section-{{ section.id }}{% if full_width %} section-background{% endif %}">
    <section data-section-id="{{ section.id }}" 
             {% if enable_slider %}
               data-section-type="slider-{{ section.id }}"
               data-url="{{ 'script.slider.js' | asset_url }}"
               data-slider-use="true"
               data-slider-breakpoint="{{ enable_slider_size }}"
             {% else %}
               data-section-type="icons-{{ section.id }}"
               data-slider-breakpoint="none"
             {% endif %}
             class="relative">
      <div class="section-padding-top section-padding-bottom section-margin-bottom{% if section.settings.padding_border %} section-border{% endif %}{% unless full_width %} section-background{% endunless %}">
        <div class="{% if enable_slider %}slider-container slider-padding {% endif %}section-card-wrapper section-card-wrapper-{{ section.settings.layout_x }}{% if block_subject %} block-layout block-layout-{{ block_layout_x }}-{{ block_layout_y }}{% endif %}">
          {%- liquid
            if block_subject
              echo block_subject
            endif
            if enable_slider
              if enable_slider_size == 'small'
                assign items_per_row_desktop = section.settings.items_per_row_desktop | minus: block_width
              else
                assign items_per_row_desktop = section.settings.items_per_row_desktop
              endif          
            else
              assign items_per_row_desktop = section.settings.items_per_row_desktop | minus: block_width
            endif
          -%}        
          <div class="{% if enable_slider %}slider {{ enable_slider_size }} scroll-bar-h {% endif %}{% unless enable_slider_size == 'large' %}container-inner{% endunless %}">
            <div class="dynamic-block-{{ section.id }} section-card-rows mobile-rows-of-{{ section.settings.items_per_row_mobile }} desktop-rows-of-{{ items_per_row_desktop }}">
              {%- liquid
                for block in section.blocks
                  case block.type
                    when 'icon', 'image', 'custom' 
                      render 'block.icon', section: section, block: block, forloop: forloop, lazy: 'eager'
                  endcase
                endfor
              -%}
            </div>
          </div>          
        </div>    
      </div>
    </section>
  </div>
{% endif %}
